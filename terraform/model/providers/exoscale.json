        "Computed": true
      },
      "zone": {
        "Type": "String",
        "Description": "Name of the availability zone for the Compute instance",
        "Computed": true
      }
    },
    "exoscale_compute_ipaddress": {
      "description": {
        "Type": "String",
        "Optional": true,
        "Description": "Description of the Elastic IP",
        "ConflictsWith": [
          "ip_address",
          "id",
          "tags"
        ]
      },
      "id": {
        "Type": "String",
        "Optional": true,
        "Description": "ID of the Elastic IP",
        "ConflictsWith": [
          "description",
          "ip_address",
          "tags"
        ]
      },
      "ip_address": {
        "Type": "String",
        "Optional": true,
        "Description": "IP Address",
        "ConflictsWith": [
          "description",
          "id",
          "tags"
        ]
      },
      "tags": {
        "Type": "Map",
        "Optional": true,
        "Description": "Map of tags (key: value)",
        "ConflictsWith": [
          "description",
          "ip_address",
          "id"
        ],
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "zone": {
        "Type": "String",
