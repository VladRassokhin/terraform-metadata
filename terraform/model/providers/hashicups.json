{
  ".schema_version": "2",
  ".sdk_type": "terraform-plugin-sdk-v2",
  "name": "hashicups",
  "type": "provider",
  "version": "v0.3.1",
  "provider": {
    "host": {
      "Type": "String",
      "Optional": true,
      "DefaultFunc": "ENV(HASHICUPS_HOST)"
    },
    "password": {
      "Type": "String",
      "Optional": true,
      "DefaultFunc": "ENV(HASHICUPS_PASSWORD)"
    },
    "username": {
      "Type": "String",
      "Optional": true,
      "DefaultFunc": "ENV(HASHICUPS_USERNAME)"
    }
  },
  "resources": {
    "hashicups_order": {
      "items": {
        "Type": "List",
        "Required": true,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "coffee": {
              "Type": "List",
              "Required": true,
              "MaxItems": 1,
              "IsBlock": true,
              "ConfigImplicitMode": "Block",
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "description": {
                    "Type": "String",
                    "Computed": true
                  },
                  "id": {
                    "Type": "Int",
                    "Required": true
                  },
                  "image": {
                    "Type": "String",
                    "Computed": true
                  },
                  "name": {
                    "Type": "String",
                    "Computed": true
                  },
                  "price": {
                    "Type": "Int",
                    "Computed": true
                  },
                  "teaser": {
                    "Type": "String",
                    "Computed": true
                  }
                }
              }
            },
            "quantity": {
              "Type": "Int",
              "Required": true
            }
          }
        }
      },
      "last_updated": {
        "Type": "String",
        "Optional": true,
        "Computed": true
      }
    }
  },
  "data-sources": {
    "hashicups_coffees": {
      "coffees": {
        "Type": "List",
        "Computed": true,
        "IsBlock": true,
        "ConfigImplicitMode": "ComputedBlock",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "description": {
              "Type": "String",
              "Computed": true
            },
            "id": {
              "Type": "Int",
              "Computed": true
            },
            "image": {
              "Type": "String",
              "Computed": true
            },
            "ingredients": {
              "Type": "List",
              "Computed": true,
              "IsBlock": true,
              "ConfigImplicitMode": "ComputedBlock",
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "ingredient_id": {
                    "Type": "Int",
                    "Computed": true
                  }
                }
              }
            },
            "name": {
              "Type": "String",
              "Computed": true
            },
            "price": {
              "Type": "Int",
              "Computed": true
            },
            "teaser": {
              "Type": "String",
              "Computed": true
            }
          }
        }
      }
    },
    "hashicups_ingredients": {
      "coffee_id": {
        "Type": "Int",
        "Required": true
      },
      "ingredients": {
        "Type": "List",
        "Computed": true,
        "IsBlock": true,
        "ConfigImplicitMode": "ComputedBlock",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "id": {
              "Type": "Int",
              "Computed": true
            },
            "name": {
              "Type": "String",
              "Computed": true
            },
            "quantity": {
              "Type": "Int",
              "Computed": true
            },
            "unit": {
              "Type": "String",
              "Computed": true
            }
          }
        }
      }
    },
    "hashicups_order": {
      "id": {
        "Type": "Int",
        "Required": true
      },
      "items": {
        "Type": "List",
        "Computed": true,
        "IsBlock": true,
        "ConfigImplicitMode": "ComputedBlock",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "coffee_description": {
              "Type": "String",
              "Computed": true
            },
            "coffee_id": {
              "Type": "Int",
              "Computed": true
            },
            "coffee_image": {
              "Type": "String",
              "Computed": true
            },
            "coffee_name": {
              "Type": "String",
              "Computed": true
            },
            "coffee_price": {
              "Type": "Int",
              "Computed": true
            },
            "coffee_teaser": {
              "Type": "String",
              "Computed": true
            },
            "quantity": {
              "Type": "Int",
              "Computed": true
            }
          }
        }
      }
    }
  }
}