{
  ".schema_version": "1",
  "name": "oktaasa",
  "type": "provider",
  "version": "v1.0.0",
  "provider": {
    "oktaasa_key": {
      "Type": "String",
      "Required": true,
      "Description": "OKTAASA API key.",
      "DefaultFunc": "ENV(OKTAASA_KEY)"
    },
    "oktaasa_secret": {
      "Type": "String",
      "Required": true,
      "Description": "OKTAASA API secret.",
      "DefaultFunc": "ENV(OKTAASA_KEY_SECRET)"
    },
    "oktaasa_team": {
      "Type": "String",
      "Required": true,
      "Description": "OKTAASA Team.",
      "DefaultFunc": "ENV(OKTAASA_TEAM)"
    }
  },
  "resources": {
    "oktaasa_assign_group": {
      "create_server_group": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "true"
        }
      },
      "group_name": {
        "Type": "String",
        "Required": true
      },
      "project_name": {
        "Type": "String",
        "Required": true
      },
      "server_access": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "true"
        }
      },
      "server_admin": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      }
    },
    "oktaasa_create_group": {
      "name": {
        "Type": "String",
        "Required": true
      }
    },
    "oktaasa_enrollment_token": {
      "description": {
        "Type": "String",
        "Required": true
      },
      "project_name": {
        "Type": "String",
        "Required": true
      },
      "token_value": {
        "Type": "String",
        "Computed": true
      }
    },
    "oktaasa_project": {
      "next_unix_gid": {
        "Type": "Int",
        "Optional": true,
        "Default": {
          "Type": "int",
          "Value": "63001"
        }
      },
      "next_unix_uid": {
        "Type": "Int",
        "Optional": true,
        "Default": {
          "Type": "int",
          "Value": "60101"
        }
      },
      "project_name": {
        "Type": "String",
        "Required": true
      }
    }
  },
  "data-sources": {}
}