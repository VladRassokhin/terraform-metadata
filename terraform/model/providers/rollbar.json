{
  ".schema_version": "2",
  ".sdk_type": "terraform-plugin-sdk-v2",
  "name": "rollbar",
  "type": "provider",
  "version": "v1.0.6",
  "provider": {
    "api_key": {
      "Type": "String",
      "Optional": true,
      "Description": "Rollbar API authentication token. Value will be sourced from environment variable `ROLLBAR_API_KEY` if set.",
      "DefaultFunc": "ENV(ROLLBAR_API_KEY)"
    },
    "api_url": {
      "Type": "String",
      "Optional": true,
      "Description": "Base URL for the Rollbar API.  Defaults to https://api.rollbar.com.  Value will be sourced from environment variable `ROLLBAR_API_URL` if set.",
      "DefaultFunc": "ENV(ROLLBAR_API_URL)"
    }
  },
  "resources": {
    "rollbar_project": {
      "account_id": {
        "Type": "Int",
        "Description": "ID of the account that owns the project",
        "Computed": true
      },
      "date_created": {
        "Type": "Int",
        "Description": "Date the project was created",
        "Computed": true
      },
      "date_modified": {
        "Type": "Int",
        "Description": "Date the project was last modified",
        "Computed": true
      },
      "name": {
        "Type": "String",
        "Required": true,
        "Description": "The human readable name for the project"
      },
      "status": {
        "Type": "String",
        "Description": "Status of the project",
        "Computed": true
      },
      "team_ids": {
        "Type": "Set",
        "Optional": true,
        "Description": "IDs of the teams assigned to the project",
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "Int"
        }
      }
    },
    "rollbar_project_access_token": {
      "access_token": {
        "Type": "String",
        "Description": "Access token for Rollbar API",
        "Computed": true
      },
      "cur_rate_limit_window_count": {
        "Type": "Int",
        "Description": "Count of calls in the current window",
        "Computed": true
      },
      "cur_rate_limit_window_start": {
        "Type": "Int",
        "Description": "Time when the current window began",
        "Computed": true
      },
      "date_created": {
        "Type": "Int",
        "Description": "Date the project was created",
        "Computed": true
      },
      "date_modified": {
        "Type": "Int",
        "Description": "Date the project was last modified",
        "Computed": true
      },
      "name": {
        "Type": "String",
        "Required": true,
        "Description": "The human readable name for the token"
      },
      "project_id": {
        "Type": "Int",
        "Required": true,
        "Description": "ID of the Rollbar project to which this token belongs"
      },
      "rate_limit_window_count": {
        "Type": "Int",
        "Optional": true,
        "Description": "Total number of calls allowed within the rate limit window",
        "Default": {
          "Type": "int",
          "Value": "0"
        }
      },
      "rate_limit_window_size": {
        "Type": "Int",
        "Optional": true,
        "Description": "Total number of seconds that makes up the rate limit window",
        "Default": {
          "Type": "int",
          "Value": "0"
        }
      },
      "scopes": {
        "Type": "List",
        "Required": true,
        "Description": "List of access scopes granted to the token.  Possible values are \"read\", \"write\", \"post_server_item\", and \"post_client_server\".",
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "status": {
        "Type": "String",
        "Optional": true,
        "Description": "Status of the token.  Possible values are \"enabled\" and \"disabled\"",
        "Default": {
          "Type": "string",
          "Value": "enabled"
        }
      }
    },
    "rollbar_team": {
      "access_level": {
        "Type": "String",
        "Optional": true,
        "Description": "The team's access level.  Must be \"standard\", \"light\", or \"view\".  Defaults to \"standard\".",
        "Default": {
          "Type": "string",
          "Value": "standard"
        }
      },
      "account_id": {
        "Type": "Int",
        "Description": "ID of account that owns the team",
        "Computed": true
      },
      "name": {
        "Type": "String",
        "Required": true,
        "Description": "Human readable name for the team"
      }
    },
    "rollbar_user": {
      "email": {
        "Type": "String",
        "Required": true,
        "Description": "The user's email address"
      },
      "status": {
        "Type": "String",
        "Description": "Status of the user.  Either `invited` or `subscribed`",
        "Computed": true
      },
      "team_ids": {
        "Type": "Set",
        "Required": true,
        "Description": "IDs of the teams to which this user belongs",
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "Int"
        }
      },
      "user_id": {
        "Type": "Int",
        "Description": "The ID of the user",
        "Computed": true
      },
      "username": {
        "Type": "String",
        "Description": "The user's username",
        "Computed": true
      }
    }
  },
  "data-sources": {
    "rollbar_project": {
      "account_id": {
        "Type": "Int",
        "Description": "ID of account that owns the project",
        "Computed": true
      },
      "date_created": {
        "Type": "Int",
        "Description": "Date the project was created",
        "Computed": true
      },
      "date_modified": {
        "Type": "Int",
        "Description": "Date the project was last modified",
        "Computed": true
      },
      "id": {
        "Type": "Int",
        "Description": "ID of project",
        "Computed": true
      },
      "name": {
        "Type": "String",
        "Required": true,
        "Description": "Human readable name for the project"
      },
      "status": {
        "Type": "String",
        "Description": "Status of the project",
        "Computed": true
      }
    },
    "rollbar_project_access_token": {
      "access_token": {
        "Type": "String",
        "Description": "API token",
        "Computed": true
      },
      "cur_rate_limit_window_count": {
        "Type": "Int",
        "Description": "Number of API hits that occurred in the current rate limit window",
        "Computed": true
      },
      "cur_rate_limit_window_start": {
        "Type": "Int",
        "Description": "Time when the current rate limit window began",
        "Computed": true
      },
      "date_created": {
        "Type": "Int",
        "Description": "Date the token was created",
        "Computed": true
      },
      "date_modified": {
        "Type": "Int",
        "Description": "Date the token was last modified",
        "Computed": true
      },
      "name": {
        "Type": "String",
        "Optional": true,
        "Description": "Name of the token"
      },
      "project_id": {
        "Type": "Int",
        "Required": true,
        "Description": "ID of a Rollbar project"
      },
      "rate_limit_window_count": {
        "Type": "Int",
        "Description": "Maximum allowed API hits during a rate limit window",
        "Computed": true
      },
      "rate_limit_window_size": {
        "Type": "Int",
        "Description": "Duration of a rate limit window",
        "Computed": true
      },
      "scopes": {
        "Type": "List",
        "Description": "Project access scopes for the token.  Possible values are \"read\", \"write\", \"post_server_item\", or \"post_client_item\".",
        "Computed": true,
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "status": {
        "Type": "String",
        "Description": "Status of the token",
        "Computed": true
      }
    },
    "rollbar_project_access_tokens": {
      "access_tokens": {
        "Type": "List",
        "Description": "List of matching project access tokens",
        "Computed": true,
        "IsBlock": true,
        "ConfigImplicitMode": "ComputedBlock",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "access_token": {
              "Type": "String",
              "Description": "API token",
              "Computed": true
            },
            "cur_rate_limit_window_count": {
              "Type": "Int",
              "Description": "Number of API hits that occurred in the current rate limit window",
              "Computed": true
            },
            "cur_rate_limit_window_start": {
              "Type": "Int",
              "Description": "Time when the current rate limit window began",
              "Computed": true
            },
            "date_created": {
              "Type": "Int",
              "Description": "Date the token was created",
              "Computed": true
            },
            "date_modified": {
              "Type": "Int",
              "Description": "Date the token was last modified",
              "Computed": true
            },
            "name": {
              "Type": "String",
              "Description": "Name of the token",
              "Computed": true
            },
            "project_id": {
              "Type": "Int",
              "Required": true,
              "Description": "ID of the project that owns the token"
            },
            "rate_limit_window_count": {
              "Type": "Int",
              "Description": "Maximum allowed API hits during a rate limit window",
              "Computed": true
            },
            "rate_limit_window_size": {
              "Type": "Int",
              "Description": "Duration of a rate limit window",
              "Computed": true
            },
            "scopes": {
              "Type": "List",
              "Description": "Project access scopes for the token",
              "Computed": true,
              "ConfigImplicitMode": "Attr",
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            },
            "status": {
              "Type": "String",
              "Description": "Status of the token",
              "Computed": true
            }
          }
        }
      },
      "prefix": {
        "Type": "String",
        "Optional": true,
        "Description": "Name of the token begins with this prefix"
      },
      "project_id": {
        "Type": "Int",
        "Required": true,
        "Description": "ID of a Rollbar project"
      }
    },
    "rollbar_projects": {
      "projects": {
        "Type": "List",
        "Description": "Rollbar projects",
        "Computed": true,
        "IsBlock": true,
        "ConfigImplicitMode": "ComputedBlock",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "account_id": {
              "Type": "Int",
              "Description": "ID of account that owns the project",
              "Computed": true
            },
            "date_created": {
              "Type": "Int",
              "Description": "Date the project was created",
              "Computed": true
            },
            "date_modified": {
              "Type": "Int",
              "Description": "Date the project was last modified",
              "Computed": true
            },
            "id": {
              "Type": "Int",
              "Description": "ID of project",
              "Computed": true
            },
            "name": {
              "Type": "String",
              "Description": "Name of project",
              "Computed": true
            },
            "status": {
              "Type": "String",
              "Description": "Status of the project",
              "Computed": true
            }
          }
        }
      }
    }
  }
}