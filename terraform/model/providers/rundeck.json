{
  ".schema_version": "2",
  ".sdk_type": "terraform-plugin-sdk-v1",
  "name": "rundeck",
  "type": "provider",
  "version": "v0.4.2",
  "provider": {
    "api_version": {
      "Type": "String",
      "Required": true,
      "Description": "API Version of the target Rundeck server.",
      "DefaultFunc": "ENV(RUNDECK_API_VERSION)"
    },
    "auth_token": {
      "Type": "String",
      "Required": true,
      "Description": "Auth token to use with the Rundeck API.",
      "DefaultFunc": "ENV(RUNDECK_AUTH_TOKEN)"
    },
    "url": {
      "Type": "String",
      "Required": true,
      "Description": "URL of the root of the target Rundeck server.",
      "DefaultFunc": "ENV(RUNDECK_URL)"
    }
  },
  "resources": {
    "rundeck_acl_policy": {
      "name": {
        "Type": "String",
        "Required": true,
        "Description": "Unique name for the ACL policy"
      },
      "policy": {
        "Type": "String",
        "Required": true,
        "Description": "YAML formatted ACL Policy string"
      }
    },
    "rundeck_job": {
      "allow_concurrent_executions": {
        "Type": "Bool",
        "Optional": true
      },
      "command": {
        "Type": "List",
        "Required": true,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "description": {
              "Type": "String",
              "Optional": true
            },
            "error_handler": {
              "Type": "List",
              "Optional": true,
              "IsBlock": true,
              "ConfigImplicitMode": "Block",
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "description": {
                    "Type": "String",
                    "Optional": true
                  },
                  "inline_script": {
                    "Type": "String",
                    "Optional": true
                  },
                  "job": {
                    "Type": "List",
                    "Optional": true,
                    "IsBlock": true,
                    "ConfigImplicitMode": "Block",
                    "Elem": {
                      "Type": "SchemaInfo",
                      "Info": {
                        "args": {
                          "Type": "String",
                          "Optional": true
                        },
                        "group_name": {
                          "Type": "String",
                          "Optional": true
                        },
                        "name": {
                          "Type": "String",
                          "Required": true
                        },
                        "node_filters": {
                          "Type": "List",
                          "Optional": true,
                          "IsBlock": true,
                          "ConfigImplicitMode": "Block",
                          "Elem": {
                            "Type": "SchemaInfo",
                            "Info": {
                              "exclude_filter": {
                                "Type": "String",
                                "Optional": true
                              },
                              "exclude_precedence": {
                                "Type": "Bool",
                                "Optional": true
                              },
                              "filter": {
                                "Type": "String",
                                "Optional": true
                              }
                            }
                          }
                        },
                        "run_for_each_node": {
                          "Type": "Bool",
                          "Optional": true
                        }
                      }
                    }
                  },
                  "keep_going_on_success": {
                    "Type": "Bool",
                    "Optional": true
                  },
                  "node_step_plugin": {
                    "Type": "List",
                    "Optional": true,
                    "IsBlock": true,
                    "ConfigImplicitMode": "Block",
                    "Elem": {
                      "Type": "SchemaInfo",
                      "Info": {
                        "config": {
                          "Type": "Map",
                          "Optional": true
                        },
                        "type": {
                          "Type": "String",
                          "Required": true
                        }
                      }
                    }
                  },
                  "script_file": {
                    "Type": "String",
                    "Optional": true
                  },
                  "script_file_args": {
                    "Type": "String",
                    "Optional": true
                  },
                  "script_interpreter": {
                    "Type": "List",
                    "Optional": true,
                    "IsBlock": true,
                    "ConfigImplicitMode": "Block",
                    "Elem": {
                      "Type": "SchemaInfo",
                      "Info": {
                        "args_quoted": {
                          "Type": "Bool",
                          "Optional": true
                        },
                        "invocation_string": {
                          "Type": "String",
                          "Optional": true
                        }
                      }
                    }
                  },
                  "shell_command": {
                    "Type": "String",
                    "Optional": true
                  },
                  "step_plugin": {
                    "Type": "List",
                    "Optional": true,
                    "IsBlock": true,
                    "ConfigImplicitMode": "Block",
                    "Elem": {
                      "Type": "SchemaInfo",
                      "Info": {
                        "config": {
                          "Type": "Map",
                          "Optional": true
                        },
                        "type": {
                          "Type": "String",
                          "Required": true
                        }
                      }
                    }
                  }
                }
              }
            },
            "inline_script": {
              "Type": "String",
              "Optional": true
            },
            "job": {
              "Type": "List",
              "Optional": true,
              "IsBlock": true,
              "ConfigImplicitMode": "Block",
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "args": {
                    "Type": "String",
                    "Optional": true
                  },
                  "group_name": {
                    "Type": "String",
                    "Optional": true
                  },
                  "name": {
                    "Type": "String",
                    "Required": true
                  },
                  "node_filters": {
                    "Type": "List",
                    "Optional": true,
                    "IsBlock": true,
                    "ConfigImplicitMode": "Block",
                    "Elem": {
                      "Type": "SchemaInfo",
                      "Info": {
                        "exclude_filter": {
                          "Type": "String",
                          "Optional": true
                        },
                        "exclude_precedence": {
                          "Type": "Bool",
                          "Optional": true
                        },
                        "filter": {
                          "Type": "String",
                          "Optional": true
                        }
                      }
                    }
                  },
                  "run_for_each_node": {
                    "Type": "Bool",
                    "Optional": true
                  }
                }
              }
            },
            "keep_going_on_success": {
              "Type": "Bool",
              "Optional": true
            },
            "node_step_plugin": {
              "Type": "List",
              "Optional": true,
              "IsBlock": true,
              "ConfigImplicitMode": "Block",
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "config": {
                    "Type": "Map",
                    "Optional": true
                  },
                  "type": {
                    "Type": "String",
                    "Required": true
                  }
                }
              }
            },
            "script_file": {
              "Type": "String",
              "Optional": true
            },
            "script_file_args": {
              "Type": "String",
              "Optional": true
            },
            "script_interpreter": {
              "Type": "List",
              "Optional": true,
              "IsBlock": true,
              "ConfigImplicitMode": "Block",
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "args_quoted": {
                    "Type": "Bool",
                    "Optional": true
                  },
                  "invocation_string": {
                    "Type": "String",
                    "Optional": true
                  }
                }
              }
            },
            "shell_command": {
              "Type": "String",
              "Optional": true
            },
            "step_plugin": {
              "Type": "List",
              "Optional": true,
              "IsBlock": true,
              "ConfigImplicitMode": "Block",
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "config": {
                    "Type": "Map",
                    "Optional": true
                  },
                  "type": {
                    "Type": "String",
                    "Required": true
                  }
                }
              }
            }
          }
        }
      },
      "command_ordering_strategy": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "node-first"
        }
      },
      "continue_next_node_on_error": {
        "Type": "Bool",
        "Optional": true
      },
      "continue_on_error": {
        "Type": "Bool",
        "Optional": true
      },
      "description": {
        "Type": "String",
        "Required": true
      },
      "execution_enabled": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "true"
        }
      },
      "global_log_filter": {
        "Type": "List",
        "Optional": true,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "config": {
              "Type": "Map",
              "Optional": true
            },
            "type": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "group_name": {
        "Type": "String",
        "Optional": true
      },
      "log_level": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "INFO"
        }
      },
      "max_thread_count": {
        "Type": "Int",
        "Optional": true,
        "Default": {
          "Type": "int",
          "Value": "1"
        }
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "node_filter_exclude_precedence": {
        "Type": "Bool",
        "Optional": true
      },
      "node_filter_exclude_query": {
        "Type": "String",
        "Optional": true
      },
      "node_filter_query": {
        "Type": "String",
        "Optional": true
      },
      "notification": {
        "Type": "List",
        "Optional": true,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "email": {
              "Type": "List",
              "Optional": true,
              "IsBlock": true,
              "ConfigImplicitMode": "Block",
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "attach_log": {
                    "Type": "Bool",
                    "Optional": true,
                    "Default": {
                      "Type": "bool",
                      "Value": "false"
                    }
                  },
                  "recipients": {
                    "Type": "List",
                    "Required": true,
                    "ConfigImplicitMode": "Attr",
                    "Elem": {
                      "Type": "SchemaElements",
                      "ElementsType": "String"
                    }
                  },
                  "subject": {
                    "Type": "String",
                    "Optional": true
                  }
                }
              }
            },
            "plugin": {
              "Type": "List",
              "Optional": true,
              "IsBlock": true,
              "ConfigImplicitMode": "Block",
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "config": {
                    "Type": "Map",
                    "Optional": true
                  },
                  "type": {
                    "Type": "String",
                    "Required": true
                  }
                }
              }
            },
            "type": {
              "Type": "String",
              "Required": true,
              "Description": "Option of `on_success`, `on_failure`, `on_start`"
            },
            "webhook_urls": {
              "Type": "List",
              "Optional": true,
              "ConfigImplicitMode": "Attr",
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            }
          }
        }
      },
      "option": {
        "Type": "List",
        "Optional": true,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "allow_multiple_values": {
              "Type": "Bool",
              "Optional": true
            },
            "default_value": {
              "Type": "String",
              "Optional": true
            },
            "description": {
              "Type": "String",
              "Optional": true
            },
            "exposed_to_scripts": {
              "Type": "Bool",
              "Optional": true
            },
            "label": {
              "Type": "String",
              "Optional": true
            },
            "multi_value_delimiter": {
              "Type": "String",
              "Optional": true
            },
            "name": {
              "Type": "String",
              "Required": true
            },
            "obscure_input": {
              "Type": "Bool",
              "Optional": true
            },
            "require_predefined_choice": {
              "Type": "Bool",
              "Optional": true
            },
            "required": {
              "Type": "Bool",
              "Optional": true
            },
            "validation_regex": {
              "Type": "String",
              "Optional": true
            },
            "value_choices": {
              "Type": "List",
              "Optional": true,
              "ConfigImplicitMode": "Attr",
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            },
            "value_choices_url": {
              "Type": "String",
              "Optional": true
            }
          }
        }
      },
      "preserve_options_order": {
        "Type": "Bool",
        "Optional": true,
        "Computed": true
      },
      "project_name": {
        "Type": "String",
        "Required": true
      },
      "rank_attribute": {
        "Type": "String",
        "Optional": true
      },
      "rank_order": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "ascending"
        }
      },
      "retry": {
        "Type": "String",
        "Optional": true
      },
      "schedule": {
        "Type": "String",
        "Optional": true
      },
      "schedule_enabled": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "true"
        }
      },
      "success_on_empty_node_filter": {
        "Type": "Bool",
        "Optional": true
      },
      "time_zone": {
        "Type": "String",
        "Optional": true
      }
    },
    "rundeck_private_key": {
      "key_material": {
        "Type": "String",
        "Required": true,
        "Description": "The private key material to store, in PEM format"
      },
      "path": {
        "Type": "String",
        "Required": true,
        "Description": "Path to the key within the key store"
      }
    },
    "rundeck_project": {
      "default_node_executor_plugin": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "jsch-ssh"
        }
      },
      "default_node_file_copier_plugin": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "jsch-scp"
        }
      },
      "description": {
        "Type": "String",
        "Optional": true,
        "Description": "Description of the project to be shown in the Rundeck UI",
        "Default": {
          "Type": "string",
          "Value": "Managed by Terraform"
        }
      },
      "extra_config": {
        "Type": "Map",
        "Optional": true,
        "Description": "Additional raw configuration parameters to include in the project configuration, with dots replaced with slashes in the key names due to limitations in Terraform's config language."
      },
      "name": {
        "Type": "String",
        "Required": true,
        "Description": "Unique name for the project"
      },
      "resource_model_source": {
        "Type": "List",
        "Required": true,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "config": {
              "Type": "Map",
              "Required": true,
              "Description": "Configuration parameters for the selected plugin"
            },
            "type": {
              "Type": "String",
              "Required": true,
              "Description": "Name of the resource model plugin to use"
            }
          }
        }
      },
      "ssh_authentication_type": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "privateKey"
        }
      },
      "ssh_key_file_path": {
        "Type": "String",
        "Optional": true
      },
      "ssh_key_storage_path": {
        "Type": "String",
        "Optional": true
      },
      "ui_url": {
        "Type": "String",
        "Computed": true
      }
    },
    "rundeck_public_key": {
      "delete": {
        "Type": "Bool",
        "Description": "True if the key should be deleted when the resource is deleted. Defaults to true if key_material is provided in the configuration.",
        "Computed": true
      },
      "key_material": {
        "Type": "String",
        "Optional": true,
        "Description": "The public key data to store, in the usual OpenSSH public key file format",
        "Computed": true
      },
      "path": {
        "Type": "String",
        "Required": true,
        "Description": "Path to the key within the key store"
      },
      "url": {
        "Type": "String",
        "Description": "URL at which the key content can be retrieved",
        "Computed": true
      }
    }
  },
  "data-sources": {}
}