{
  ".schema_version": "1",
  "name": "cherryservers",
  "type": "provider",
  "version": "v1.0.0",
  "provider": {
    "auth_token": {
      "Type": "String",
      "Required": true,
      "Description": "The API token",
      "DefaultFunc": "ENV(CHERRY_AUTH_TOKEN)"
    }
  },
  "resources": {
    "cherryservers_ip": {
      "a_record": {
        "Type": "String",
        "Computed": true
      },
      "address": {
        "Type": "String",
        "Computed": true
      },
      "cidr": {
        "Type": "String",
        "Computed": true
      },
      "gateway": {
        "Type": "String",
        "Computed": true
      },
      "project_id": {
        "Type": "String",
        "Required": true
      },
      "ptr_record": {
        "Type": "String",
        "Computed": true
      },
      "region": {
        "Type": "String",
        "Required": true
      },
      "routed_to": {
        "Type": "String",
        "Optional": true
      },
      "routed_to_hostname": {
        "Type": "String",
        "Optional": true
      },
      "routed_to_ip": {
        "Type": "String",
        "Optional": true
      },
      "type": {
        "Type": "String",
        "Computed": true
      }
    },
    "cherryservers_project": {
      "name": {
        "Type": "String",
        "Required": true
      },
      "project_id": {
        "Type": "String",
        "Computed": true
      },
      "team_id": {
        "Type": "String",
        "Required": true
      }
    },
    "cherryservers_server": {
      "hostname": {
        "Type": "String",
        "Required": true
      },
      "image": {
        "Type": "String",
        "Required": true
      },
      "ip_addresses_ids": {
        "Type": "List",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "plan_id": {
        "Type": "String",
        "Required": true
      },
      "power_state": {
        "Type": "String",
        "Computed": true
      },
      "primary_ip": {
        "Type": "String",
        "Computed": true
      },
      "private_ip": {
        "Type": "String",
        "Computed": true
      },
      "project_id": {
        "Type": "String",
        "Required": true
      },
      "region": {
        "Type": "String",
        "Required": true
      },
      "ssh_keys_ids": {
        "Type": "List",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "state": {
        "Type": "String",
        "Computed": true
      },
      "tags": {
        "Type": "Map",
        "Optional": true
      },
      "user_data": {
        "Type": "String",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      }
    },
    "cherryservers_ssh": {
      "created": {
        "Type": "String",
        "Computed": true
      },
      "fingerprint": {
        "Type": "String",
        "Computed": true
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "public_key": {
        "Type": "String",
        "Required": true
      },
      "updated": {
        "Type": "String",
        "Computed": true
      }
    }
