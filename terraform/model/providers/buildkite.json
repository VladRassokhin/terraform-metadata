{
  ".schema_version": "2",
  ".sdk_type": "terraform-plugin-sdk-v2",
  "name": "buildkite",
  "type": "provider",
  "version": "v0.3.0",
  "provider": {
    "api_token": {
      "Type": "String",
      "Required": true,
      "Description": "API token with GraphQL access and `write_pipelines, read_pipelines` scopes",
      "DefaultFunc": "ENV(BUILDKITE_API_TOKEN)"
    },
    "organization": {
      "Type": "String",
      "Required": true,
      "Description": "The Buildkite organization ID",
      "DefaultFunc": "ENV(BUILDKITE_ORGANIZATION)"
    }
  },
  "resources": {
    "buildkite_agent_token": {
      "description": {
        "Type": "String",
        "Optional": true
      },
      "token": {
        "Type": "String",
        "Computed": true
      },
      "uuid": {
        "Type": "String",
        "Computed": true
      }
    },
    "buildkite_pipeline": {
      "branch_configuration": {
        "Type": "String",
        "Optional": true,
        "Computed": true
      },
      "cancel_intermediate_builds": {
        "Type": "Bool",
        "Optional": true,
        "Computed": true
      },
      "cancel_intermediate_builds_branch_filter": {
        "Type": "String",
        "Optional": true,
        "Computed": true
      },
      "default_branch": {
        "Type": "String",
        "Optional": true,
        "Computed": true
      },
      "description": {
        "Type": "String",
        "Optional": true,
        "Computed": true
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "provider_settings": {
        "Type": "List",
        "Optional": true,
        "Computed": true,
        "MaxItems": 1,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "build_branches": {
              "Type": "Bool",
              "Optional": true,
              "Computed": true
            },
            "build_pull_request_forks": {
              "Type": "Bool",
              "Optional": true,
              "Computed": true
            },
            "build_pull_request_ready_for_review": {
              "Type": "Bool",
              "Optional": true,
              "Computed": true
            },
            "build_pull_requests": {
              "Type": "Bool",
              "Optional": true,
              "Computed": true
            },
            "build_tags": {
              "Type": "Bool",
              "Optional": true,
              "Computed": true
            },
            "cancel_deleted_branch_builds": {
              "Type": "Bool",
              "Optional": true,
              "Computed": true
            },
            "prefix_pull_request_fork_branch_names": {
              "Type": "Bool",
              "Optional": true,
              "Computed": true
            },
            "publish_blocked_as_pending": {
              "Type": "Bool",
              "Optional": true,
              "Computed": true
            },
            "publish_commit_status": {
              "Type": "Bool",
              "Optional": true,
              "Computed": true
            },
            "publish_commit_status_per_step": {
              "Type": "Bool",
              "Optional": true,
              "Computed": true
            },
            "pull_request_branch_filter_configuration": {
              "Type": "String",
              "Optional": true,
              "Computed": true
            },
            "pull_request_branch_filter_enabled": {
              "Type": "Bool",
              "Optional": true,
              "Computed": true
            },
            "separate_pull_request_statuses": {
              "Type": "Bool",
              "Optional": true,
              "Computed": true
            },
            "skip_pull_request_builds_for_existing_commits": {
              "Type": "Bool",
              "Optional": true,
              "Computed": true
            },
            "trigger_mode": {
              "Type": "String",
              "Optional": true,
              "Computed": true
            }
          }
        }
      },
      "repository": {
        "Type": "String",
        "Required": true
      },
      "skip_intermediate_builds": {
        "Type": "Bool",
        "Optional": true,
        "Computed": true
      },
      "skip_intermediate_builds_branch_filter": {
        "Type": "String",
        "Optional": true,
        "Computed": true
      },
      "slug": {
        "Type": "String",
        "Computed": true
      },
      "steps": {
        "Type": "String",
        "Required": true
      },
      "team": {
        "Type": "Set",
        "Optional": true,
        "ConfigExplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "access_level": {
              "Type": "String",
              "Required": true
            },
            "slug": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "webhook_url": {
        "Type": "String",
        "Computed": true
      }
    },
    "buildkite_pipeline_schedule": {
      "branch": {
        "Type": "String",
        "Required": true
      },
      "commit": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "HEAD"
        }
      },
      "cronline": {
        "Type": "String",
        "Required": true
      },
      "enabled": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "true"
        }
      },
      "env": {
        "Type": "Map",
        "Optional": true,
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "label": {
        "Type": "String",
        "Required": true
      },
      "message": {
        "Type": "String",
        "Optional": true,
        "Computed": true
      },
      "pipeline_id": {
        "Type": "String",
        "Required": true
      },
      "uuid": {
        "Type": "String",
        "Computed": true
      }
    },
    "buildkite_team": {
      "default_member_role": {
        "Type": "String",
        "Required": true
      },
      "default_team": {
        "Type": "Bool",
        "Required": true
      },
      "description": {
        "Type": "String",
        "Optional": true
      },
      "members_can_create_pipelines": {
        "Type": "Bool",
        "Optional": true
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "privacy": {
        "Type": "String",
        "Required": true
      },
      "slug": {
        "Type": "String",
        "Computed": true
      },
      "uuid": {
        "Type": "String",
        "Computed": true
      }
    }
  },
  "data-sources": {
    "buildkite_pipeline": {
      "default_branch": {
        "Type": "String",
        "Computed": true
      },
      "description": {
        "Type": "String",
        "Computed": true
      },
      "name": {
        "Type": "String",
        "Computed": true
      },
      "repository": {
        "Type": "String",
        "Computed": true
      },
      "slug": {
        "Type": "String",
        "Required": true
      },
      "webhook_url": {
        "Type": "String",
        "Computed": true
      }
    }
  }
}