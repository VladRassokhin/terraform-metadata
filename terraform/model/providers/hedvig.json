{
  ".schema_version": "2",
  ".sdk_type": "terraform",
  "name": "hedvig",
  "type": "provider",
  "version": "v1.2.0",
  "provider": {
    "node": {
      "Type": "String",
      "Required": true
    },
    "password": {
      "Type": "String",
      "Required": true,
      "DefaultFunc": "ENV(HV_TESTPASS)"
    },
    "username": {
      "Type": "String",
      "Required": true,
      "DefaultFunc": "ENV(HV_TESTUSER)"
    }
  },
  "resources": {
    "hedvig_access": {
      "address": {
        "Type": "String",
        "Required": true
      },
      "host": {
        "Type": "String",
        "Required": true
      },
      "type": {
        "Type": "String",
        "Required": true
      },
      "vdisk": {
        "Type": "String",
        "Required": true
      }
    },
    "hedvig_lun": {
      "controller": {
        "Type": "String",
        "Required": true
      },
      "vdisk": {
        "Type": "String",
        "Required": true
      }
    },
    "hedvig_mount": {
      "controller": {
        "Type": "String",
        "Required": true
      },
      "vdisk": {
        "Type": "String",
        "Required": true
      }
    },
    "hedvig_vdisk": {
      "blocksize": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "4096"
        }
      },
      "cacheenabled": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "false"
        }
      },
      "clusteredfilesystem": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "false"
        }
      },
      "compressed": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "false"
        }
      },
      "deduplication": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "description": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string"
        }
      },
      "encryption": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "false"
        }
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "replicationfactor": {
        "Type": "Int",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "3"
        }
      },
      "replicationpolicy": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "Agnostic"
        }
      },
      "residence": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "HDD"
        }
      },
      "scsi3pr": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "false"
        }
      },
      "size": {
        "Type": "Int",
        "Required": true
      },
      "type": {
        "Type": "String",
        "Required": true
      }
    }
  },
  "data-sources": {}
}