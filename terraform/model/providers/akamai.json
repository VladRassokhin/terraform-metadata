{
  ".schema_version": "2",
  ".sdk_type": "terraform-plugin-sdk-v2",
  "name": "akamai",
  "type": "provider",
  "version": "v1.8.0",
  "provider": {
    "cache_enabled": {
      "Type": "Bool",
      "Optional": true,
      "Default": {
        "Type": "bool",
        "Value": "true"
      }
    },
    "config": {
      "Type": "Set",
      "Optional": true,
      "MaxItems": 1,
      "IsBlock": true,
      "ConfigImplicitMode": "Block",
      "Elem": {
        "Type": "SchemaInfo",
        "Info": {
          "access_token": {
            "Type": "String",
            "Optional": true,
            "DefaultFunc": "UNKNOWN"
          },
          "account_key": {
            "Type": "String",
            "Optional": true,
            "DefaultFunc": "UNKNOWN"
          },
          "client_secret": {
            "Type": "String",
            "Optional": true,
            "DefaultFunc": "UNKNOWN"
          },
          "client_token": {
            "Type": "String",
            "Optional": true,
            "DefaultFunc": "UNKNOWN"
          },
          "host": {
            "Type": "String",
            "Optional": true,
            "DefaultFunc": "UNKNOWN"
          },
          "max_body": {
            "Type": "Int",
            "Optional": true,
            "DefaultFunc": "UNKNOWN"
          }
        }
      }
    },
    "config_section": {
      "Type": "String",
      "Optional": true,
      "Description": "The section of the edgerc file to use for configuration"
    },
    "edgerc": {
      "Type": "String",
      "Optional": true,
      "DefaultFunc": "ENV(EDGERC)"
    }
  },
  "resources": {},
  "data-sources": {}
}