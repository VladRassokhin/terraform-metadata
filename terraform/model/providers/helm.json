{
  ".schema_version": "2",
  ".sdk_type": "terraform-plugin-sdk-v2",
  "name": "helm",
  "type": "provider",
  "version": "v2.1.2",
  "provider": {
    "debug": {
      "Type": "Bool",
      "Optional": true,
      "Description": "Debug indicates whether or not Helm is running in Debug mode.",
      "DefaultFunc": "ENV(HELM_DEBUG)"
    },
    "experiments": {
      "Type": "List",
      "Optional": true,
      "Description": "Enable and disable experimental features.",
      "MaxItems": 1,
      "IsBlock": true,
      "ConfigImplicitMode": "Block",
      "Elem": {
        "Type": "SchemaInfo",
        "Info": {
          "manifest": {
            "Type": "Bool",
            "Optional": true,
            "Description": "Enable full diff by storing the rendered manifest in the state.",
            "DefaultFunc": "UNKNOWN"
          }
        }
      }
    },
    "helm_driver": {
      "Type": "String",
      "Optional": true,
      "Description": "The backend storage driver. Values are: configmap, secret, memory, sql",
      "DefaultFunc": "ENV(HELM_DRIVER)"
    },
    "kubernetes": {
      "Type": "List",
      "Optional": true,
      "Description": "Kubernetes configuration.",
      "MaxItems": 1,
      "IsBlock": true,
      "ConfigImplicitMode": "Block",
      "Elem": {
        "Type": "SchemaInfo",
        "Info": {
          "client_certificate": {
            "Type": "String",
            "Optional": true,
            "Description": "PEM-encoded client certificate for TLS authentication.",
            "DefaultFunc": "ENV(KUBE_CLIENT_CERT_DATA)"
          },
          "client_key": {
            "Type": "String",
            "Optional": true,
            "Description": "PEM-encoded client certificate key for TLS authentication.",
            "DefaultFunc": "ENV(KUBE_CLIENT_KEY_DATA)"
          },
          "cluster_ca_certificate": {
            "Type": "String",
            "Optional": true,
            "Description": "PEM-encoded root certificates bundle for TLS authentication.",
            "DefaultFunc": "ENV(KUBE_CLUSTER_CA_CERT_DATA)"
          },
          "config_context": {
            "Type": "String",
            "Optional": true,
            "DefaultFunc": "ENV(KUBE_CTX)"
          },
          "config_context_auth_info": {
            "Type": "String",
            "Optional": true,
            "DefaultFunc": "ENV(KUBE_CTX_AUTH_INFO)"
          },
          "config_context_cluster": {
            "Type": "String",
            "Optional": true,
            "DefaultFunc": "ENV(KUBE_CTX_CLUSTER)"
          },
          "config_path": {
            "Type": "String",
            "Optional": true,
            "Description": "Path to the kube config file. Can be set with KUBE_CONFIG_PATH.",
            "DefaultFunc": "ENV(KUBE_CONFIG_PATH)",
            "ConflictsWith": [
              "kubernetes.0.config_paths"
            ]
          },
          "config_paths": {
            "Type": "List",
            "Optional": true,
            "Description": "A list of paths to kube config files. Can be set with KUBE_CONFIG_PATHS environment variable.",
            "ConfigImplicitMode": "Attr",
            "Elem": {
              "Type": "SchemaElements",
              "ElementsType": "String"
            }
          },
          "exec": {
            "Type": "List",
            "Optional": true,
            "MaxItems": 1,
            "IsBlock": true,
            "ConfigImplicitMode": "Block",
            "Elem": {
              "Type": "SchemaInfo",
              "Info": {
                "api_version": {
                  "Type": "String",
                  "Required": true
                },
                "args": {
                  "Type": "List",
                  "Optional": true,
                  "ConfigImplicitMode": "Attr",
                  "Elem": {
                    "Type": "SchemaElements",
                    "ElementsType": "String"
                  }
                },
                "command": {
                  "Type": "String",
                  "Required": true
                },
                "env": {
                  "Type": "Map",
                  "Optional": true,
                  "ConfigImplicitMode": "Attr",
                  "Elem": {
                    "Type": "SchemaElements",
                    "ElementsType": "String"
                  }
                }
              }
            }
          },
          "host": {
            "Type": "String",
            "Optional": true,
            "Description": "The hostname (in form of URI) of Kubernetes master.",
            "DefaultFunc": "ENV(KUBE_HOST)"
          },
          "insecure": {
            "Type": "Bool",
            "Optional": true,
            "Description": "Whether server should be accessed without verifying the TLS certificate.",
            "DefaultFunc": "ENV(KUBE_INSECURE)"
          },
          "password": {
            "Type": "String",
            "Optional": true,
            "Description": "The password to use for HTTP basic authentication when accessing the Kubernetes master endpoint.",
            "DefaultFunc": "ENV(KUBE_PASSWORD)"
          },
          "token": {
            "Type": "String",
            "Optional": true,
            "Description": "Token to authenticate an service account",
            "DefaultFunc": "ENV(KUBE_TOKEN)"
          },
          "username": {
            "Type": "String",
            "Optional": true,
            "Description": "The username to use for HTTP basic authentication when accessing the Kubernetes master endpoint.",
            "DefaultFunc": "ENV(KUBE_USER)"
          }
        }
      }
    },
    "plugins_path": {
      "Type": "String",
      "Optional": true,
      "Description": "The path to the helm plugins directory",
      "DefaultFunc": "ENV(HELM_PLUGINS)"
    },
    "registry_config_path": {
      "Type": "String",
      "Optional": true,
      "Description": "The path to the registry config file",
      "DefaultFunc": "ENV(HELM_REGISTRY_CONFIG)"
    },
    "repository_cache": {
      "Type": "String",
      "Optional": true,
      "Description": "The path to the file containing cached repository indexes",
      "DefaultFunc": "ENV(HELM_REPOSITORY_CACHE)"
    },
    "repository_config_path": {
      "Type": "String",
      "Optional": true,
      "Description": "The path to the file containing repository names and URLs",
      "DefaultFunc": "ENV(HELM_REPOSITORY_CONFIG)"
    }
  },
  "resources": {
    "helm_release": {
      "atomic": {
        "Type": "Bool",
        "Optional": true,
        "Description": "If set, installation process purges chart on fail. The wait flag will be set automatically if atomic is used",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "chart": {
        "Type": "String",
        "Required": true,
        "Description": "Chart name to be installed. A path may be used."
      },
      "cleanup_on_fail": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Allow deletion of new resources created in this upgrade when upgrade fails",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "create_namespace": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Create the namespace if it does not exist",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "dependency_update": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Run helm dependency update before installing the chart",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "description": {
        "Type": "String",
        "Optional": true,
        "Description": "Add a custom description"
      },
      "devel": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Use chart development versions, too. Equivalent to version '\u003e0.0.0-0'. If `version` is set, this is ignored"
      },
      "disable_crd_hooks": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Prevent CRD hooks from, running, but run other hooks.  See helm install --no-crd-hook",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "disable_openapi_validation": {
        "Type": "Bool",
        "Optional": true,
        "Description": "If set, the installation process will not validate rendered templates against the Kubernetes OpenAPI Schema",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "disable_webhooks": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Prevent hooks from running.",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "force_update": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Force resource update through delete/recreate if needed.",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "keyring": {
        "Type": "String",
        "Optional": true,
        "Description": "Location of public keys used for verification. Used only if `verify` is true",
        "Default": {
          "Type": "string",
          "Value": "/home/cageyv/.gnupg/pubring.gpg"
        }
      },
      "lint": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Run helm lint when planning",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "manifest": {
        "Type": "String",
        "Description": "The rendered manifest as JSON.",
        "Computed": true
      },
      "max_history": {
        "Type": "Int",
        "Optional": true,
        "Description": "Limit the maximum number of revisions saved per release. Use 0 for no limit",
        "Default": {
          "Type": "int",
          "Value": "0"
        }
      },
      "metadata": {
        "Type": "List",
        "Description": "Status of the deployed release.",
        "Computed": true,
        "IsBlock": true,
        "ConfigImplicitMode": "ComputedBlock",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "app_version": {
              "Type": "String",
              "Description": "The version number of the application being deployed.",
              "Computed": true
            },
            "chart": {
              "Type": "String",
              "Description": "The name of the chart.",
              "Computed": true
            },
            "name": {
              "Type": "String",
              "Description": "Name is the name of the release.",
              "Computed": true
            },
            "namespace": {
              "Type": "String",
              "Description": "Namespace is the kubernetes namespace of the release.",
              "Computed": true
            },
            "revision": {
              "Type": "Int",
              "Description": "Version is an int32 which represents the version of the release.",
              "Computed": true
            },
            "values": {
              "Type": "String",
              "Description": "Set of extra values, added to the chart. The sensitive data is cloaked. JSON encoded.",
              "Computed": true
            },
            "version": {
              "Type": "String",
              "Description": "A SemVer 2 conformant version string of the chart.",
              "Computed": true
            }
          }
        }
      },
      "name": {
        "Type": "String",
        "Required": true,
        "Description": "Release name."
      },
      "namespace": {
        "Type": "String",
        "Optional": true,
        "Description": "Namespace to install the release into.",
        "DefaultFunc": "ENV(HELM_NAMESPACE)"
      },
      "postrender": {
        "Type": "List",
        "Optional": true,
        "Description": "Postrender command configuration.",
        "MaxItems": 1,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "binary_path": {
              "Type": "String",
              "Required": true,
              "Description": "The command binary path."
            }
          }
        }
      },
      "recreate_pods": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Perform pods restart during upgrade/rollback",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "render_subchart_notes": {
        "Type": "Bool",
        "Optional": true,
        "Description": "If set, render subchart notes along with the parent",
        "Default": {
          "Type": "bool",
          "Value": "true"
        }
      },
      "replace": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Re-use the given name, even if that name is already used. This is unsafe in production",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "repository": {
        "Type": "String",
        "Optional": true,
        "Description": "Repository where to locate the requested chart. If is a URL the chart is installed without installing the repository."
      },
      "repository_ca_file": {
        "Type": "String",
        "Optional": true,
        "Description": "The Repositories CA File"
      },
      "repository_cert_file": {
        "Type": "String",
        "Optional": true,
        "Description": "The repositories cert file"
      },
      "repository_key_file": {
        "Type": "String",
        "Optional": true,
        "Description": "The repositories cert key file"
      },
      "repository_password": {
        "Type": "String",
        "Optional": true,
        "Description": "Password for HTTP basic authentication"
      },
      "repository_username": {
        "Type": "String",
        "Optional": true,
        "Description": "Username for HTTP basic authentication"
      },
      "reset_values": {
        "Type": "Bool",
        "Optional": true,
        "Description": "When upgrading, reset the values to the ones built into the chart",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "reuse_values": {
        "Type": "Bool",
        "Optional": true,
        "Description": "When upgrading, reuse the last release's values and merge in any overrides. If 'reset_values' is specified, this is ignored",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "set": {
        "Type": "Set",
        "Optional": true,
        "Description": "Custom values to be merged with the values.",
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "name": {
              "Type": "String",
              "Required": true
            },
            "type": {
              "Type": "String",
              "Optional": true
            },
            "value": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "set_sensitive": {
        "Type": "Set",
        "Optional": true,
        "Description": "Custom sensitive values to be merged with the values.",
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "name": {
              "Type": "String",
              "Required": true
            },
            "type": {
              "Type": "String",
              "Optional": true
            },
            "value": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "skip_crds": {
        "Type": "Bool",
        "Optional": true,
        "Description": "If set, no CRDs will be installed. By default, CRDs are installed if not already present",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "status": {
        "Type": "String",
        "Description": "Status of the release.",
        "Computed": true
      },
      "timeout": {
        "Type": "Int",
        "Optional": true,
        "Description": "Time in seconds to wait for any individual kubernetes operation.",
        "Default": {
          "Type": "int",
          "Value": "300"
        }
      },
      "values": {
        "Type": "List",
        "Optional": true,
        "Description": "List of values in raw yaml format to pass to helm.",
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "verify": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Verify the package before installing it.",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "version": {
        "Type": "String",
        "Optional": true,
        "Description": "Specify the exact chart version to install. If this is not specified, the latest version is installed.",
        "Computed": true
      },
      "wait": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Will wait until all resources are in a ready state before marking the release as successful.",
        "Default": {
          "Type": "bool",
          "Value": "true"
        }
      },
      "wait_for_jobs": {
        "Type": "Bool",
        "Optional": true,
        "Description": "If wait is enabled, will wait until all Jobs have been completed before marking the release as successful.",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      }
    }
  },
  "data-sources": {}
}