{
  ".schema_version": "1",
  "name": "bitbucket",
  "type": "provider",
  "version": "v1.2.0",
  "provider": {
    "password": {
      "Type": "String",
      "Required": true,
      "DefaultFunc": "ENV(BITBUCKET_PASSWORD)"
    },
    "username": {
      "Type": "String",
      "Required": true,
      "DefaultFunc": "ENV(BITBUCKET_USERNAME)"
    }
  },
  "resources": {
    "bitbucket_branch_restriction": {
      "groups": {
        "Type": "Set",
        "Optional": true,
        "IsBlock": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "owner": {
              "Type": "String",
              "Required": true
            },
            "slug": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "kind": {
        "Type": "String",
        "Required": true
      },
      "owner": {
        "Type": "String",
        "Required": true
      },
      "pattern": {
        "Type": "String",
        "Required": true
      },
      "repository": {
        "Type": "String",
        "Required": true
      },
      "users": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "value": {
        "Type": "Int",
        "Optional": true
      }
    },
    "bitbucket_default_reviewers": {
      "owner": {
        "Type": "String",
        "Required": true
      },
      "repository": {
        "Type": "String",
        "Required": true
      },
      "reviewers": {
        "Type": "Set",
        "Required": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      }
    },
    "bitbucket_hook": {
      "active": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "true"
        }
      },
      "description": {
        "Type": "String",
        "Required": true
      },
      "events": {
        "Type": "Set",
        "Required": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "owner": {
        "Type": "String",
        "Required": true
      },
      "repository": {
        "Type": "String",
        "Required": true
      },
      "skip_cert_verification": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "true"
        }
      },
      "url": {
        "Type": "String",
        "Required": true
      },
      "uuid": {
        "Type": "String",
        "Computed": true
      }
    },
    "bitbucket_project": {
      "description": {
        "Type": "String",
        "Optional": true
      },
      "is_private": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "true"
        }
      },
      "key": {
        "Type": "String",
        "Required": true
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "owner": {
        "Type": "String",
        "Required": true
      }
    },
    "bitbucket_repository": {
      "clone_https": {
        "Type": "String",
        "Computed": true
      },
      "clone_ssh": {
        "Type": "String",
        "Computed": true
      },
      "description": {
        "Type": "String",
        "Optional": true
      },
      "fork_policy": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "allow_forks"
        }
      },
      "has_issues": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "has_wiki": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "is_private": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "true"
        }
      },
      "language": {
        "Type": "String",
        "Optional": true
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "owner": {
        "Type": "String",
        "Required": true
      },
      "pipelines_enabled": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "project_key": {
        "Type": "String",
        "Optional": true
      },
      "scm": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "git"
        }
      },
      "slug": {
        "Type": "String",
        "Optional": true,
        "Computed": true
      },
      "website": {
        "Type": "String",
        "Optional": true
      }
    },
    "bitbucket_repository_variable": {
      "key": {
        "Type": "String",
        "Required": true
      },
      "repository": {
        "Type": "String",
        "Required": true
      },
      "secured": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "true"
        }
      },
      "uuid": {
        "Type": "String",
        "Computed": true
      },
      "value": {
        "Type": "String",
        "Required": true
      }
    }
  },
  "data-sources": {
    "bitbucket_user": {
      "display_name": {
        "Type": "String",
        "Optional": true
      },
      "nickname": {
        "Type": "String",
        "Optional": true
      },
      "username": {
        "Type": "String",
        "Required": true
      },
      "uuid": {
        "Type": "String",
        "Computed": true
      }
    }
  }
}