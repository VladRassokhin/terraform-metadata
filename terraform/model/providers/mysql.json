{
  ".schema_version": "1",
  "name": "mysql",
  "type": "provider",
  "version": "v1.9.0",
  "provider": {
    "authentication_plugin": {
      "Type": "String",
      "Optional": true,
      "Default": {
        "Type": "string",
        "Value": "native"
      }
    },
    "endpoint": {
      "Type": "String",
      "Required": true,
      "DefaultFunc": "ENV(MYSQL_ENDPOINT)"
    },
    "max_conn_lifetime_sec": {
      "Type": "Int",
      "Optional": true
    },
    "max_open_conns": {
      "Type": "Int",
      "Optional": true
    },
    "password": {
      "Type": "String",
      "Optional": true,
      "DefaultFunc": "ENV(MYSQL_PASSWORD)"
    },
    "proxy": {
      "Type": "String",
      "Optional": true,
      "DefaultFunc": "MENV(ALL_PROXY,all_proxy)"
    },
    "tls": {
      "Type": "String",
      "Optional": true,
      "DefaultFunc": "ENV(MYSQL_TLS_CONFIG)"
    },
    "username": {
      "Type": "String",
      "Required": true,
      "DefaultFunc": "ENV(MYSQL_USERNAME)"
    }
  },
  "resources": {
    "mysql_database": {
      "default_character_set": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "utf8"
        }
      },
      "default_collation": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "utf8_general_ci"
        }
      },
      "name": {
        "Type": "String",
        "Required": true
      }
    },
    "mysql_grant": {
      "database": {
        "Type": "String",
        "Required": true
      },
      "grant": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "host": {
        "Type": "String",
        "Optional": true,
        "ConflictsWith": [
          "role"
        ],
        "Default": {
          "Type": "string",
          "Value": "localhost"
        }
      },
      "privileges": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "role": {
        "Type": "String",
        "Optional": true,
        "ConflictsWith": [
          "user",
          "host"
        ]
      },
      "roles": {
        "Type": "Set",
        "Optional": true,
        "ConflictsWith": [
          "privileges"
        ],
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "table": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "*"
        }
      },
      "tls_option": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "NONE"
        }
      },
      "user": {
        "Type": "String",
        "Optional": true,
        "ConflictsWith": [
          "role"
        ]
      }
    },
    "mysql_role": {
      "name": {
        "Type": "String",
        "Required": true
      }
    },
    "mysql_user": {
      "auth_plugin": {
        "Type": "String",
        "Optional": true,
        "ConflictsWith": [
          "plaintext_password",
          "password"
        ]
      },
      "host": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "localhost"
        }
      },
      "password": {
        "Type": "String",
        "Optional": true,
        "ConflictsWith": [
          "plaintext_password"
        ],
        "Deprecated": "Please use plaintext_password instead"
      },
      "plaintext_password": {
        "Type": "String",
        "Optional": true
      },
      "tls_option": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "NONE"
        }
      },
      "user": {
        "Type": "String",
        "Required": true
      }
    },
    "mysql_user_password": {
      "encrypted_password": {
        "Type": "String",
        "Computed": true
      },
      "host": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "localhost"
        }
      },
      "key_fingerprint": {
        "Type": "String",
        "Computed": true
      },
      "pgp_key": {
        "Type": "String",
        "Required": true
      },
      "user": {
        "Type": "String",
        "Required": true
      }
    }
  },
  "data-sources": {}
}