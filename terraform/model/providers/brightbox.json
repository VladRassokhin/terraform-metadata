{
  ".schema_version": "2",
  ".sdk_type": "terraform-plugin-sdk-v2",
  "name": "brightbox",
  "type": "provider",
  "version": "v2.0.3",
  "provider": {
    "account": {
      "Type": "String",
      "Optional": true,
      "Description": "Brightbox Cloud Account to operate on",
      "DefaultFunc": "ENV(BRIGHTBOX_ACCOUNT)"
    },
    "apiclient": {
      "Type": "String",
      "Optional": true,
      "Description": "Brightbox Cloud API Client/OAuth Application ID",
      "DefaultFunc": "ENV(BRIGHTBOX_CLIENT)"
    },
    "apisecret": {
      "Type": "String",
      "Optional": true,
      "Description": "Brightbox Cloud API Client/OAuth Application Secret",
      "DefaultFunc": "ENV(BRIGHTBOX_CLIENT_SECRET)"
    },
    "apiurl": {
      "Type": "String",
      "Optional": true,
      "Description": "Brightbox Cloud Api URL for selected Region",
      "DefaultFunc": "ENV(BRIGHTBOX_API_URL)"
    },
    "orbit_url": {
      "Type": "String",
      "Optional": true,
      "Description": "Brightbox Cloud Orbit URL for selected Region",
      "DefaultFunc": "ENV(BRIGHTBOX_ORBIT_URL)"
    },
    "password": {
      "Type": "String",
      "Optional": true,
      "Description": "Brightbox Cloud Password for User Name",
      "DefaultFunc": "ENV(BRIGHTBOX_PASSWORD)"
    },
    "username": {
      "Type": "String",
      "Optional": true,
      "Description": "Brightbox Cloud User Name",
      "DefaultFunc": "ENV(BRIGHTBOX_USER_NAME)"
    }
  },
  "resources": {
    "brightbox_api_client": {
      "__timeouts__": [
        "create",
        "delete"
      ],
      "account": {
        "Type": "String",
        "Description": "The account the API client relates to",
        "Computed": true
      },
      "description": {
        "Type": "String",
        "Optional": true,
        "Description": "Verbose Description of this client"
      },
      "name": {
        "Type": "String",
        "Optional": true,
        "Description": "Human Readable Name"
      },
      "permissions_group": {
        "Type": "String",
        "Optional": true,
        "Description": "Summary of the permissions granted to the client (full, storage)",
        "Default": {
          "Type": "string",
          "Value": "full"
        }
      },
      "secret": {
        "Type": "String",
        "Description": "A shared secret the client must present when authenticating",
        "Computed": true
      }
    },
    "brightbox_cloudip": {
      "__timeouts__": [
        "create",
        "delete"
      ],
      "fqdn": {
        "Type": "String",
        "Description": "Full Domain name entry for the Cloud IP",
        "Computed": true
      },
      "locked": {
        "Type": "String",
        "Description": "No lock on Cloud IPs",
        "Computed": true,
        "Deprecated": "No lock on Cloud IPs"
      },
      "name": {
        "Type": "String",
        "Optional": true,
        "Description": "Name assigned to the Cloud IP"
      },
      "port_translator": {
        "Type": "Set",
        "Optional": true,
        "Description": "Array of Port Translators",
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "incoming": {
              "Type": "Int",
              "Required": true,
              "Description": "Incoming Port"
            },
            "outgoing": {
              "Type": "Int",
              "Required": true,
              "Description": "Outgoing Port"
            },
            "protocol": {
              "Type": "String",
              "Required": true,
              "Description": "Transport protocol to port translate (tcp/udp)"
            }
          }
        }
      },
      "public_ip": {
        "Type": "String",
        "Description": "Old alias of the IPv4 address",
        "Computed": true
      },
      "public_ipv4": {
        "Type": "String",
        "Description": "IPv4 address",
        "Computed": true
      },
      "public_ipv6": {
        "Type": "String",
        "Description": "IPv6 address",
        "Computed": true
      },
      "reverse_dns": {
        "Type": "String",
        "Optional": true,
        "Description": "Reverse DNS entry for the Cloud IP",
        "Computed": true
      },
      "status": {
        "Type": "String",
        "Description": "Current state of the Cloud IP",
        "Computed": true
      },
      "target": {
        "Type": "String",
        "Optional": true,
        "Description": "The object this Cloud IP maps to"
      }
    },
    "brightbox_config_map": {
      "__timeouts__": [
        "create",
        "delete"
      ],
      "data": {
        "Type": "Map",
        "Required": true,
        "Description": "keys/values making up the ConfigMap",
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "name": {
        "Type": "String",
        "Optional": true,
        "Description": "User editable label"
      }
    },
    "brightbox_database_server": {
      "__timeouts__": [
        "create",
        "delete"
      ],
      "admin_password": {
        "Type": "String",
        "Description": "Initial password required to login, only available at creation or following a password reset request",
        "Computed": true
      },
      "admin_username": {
        "Type": "String",
        "Description": "Initial username required to login",
        "Computed": true
      },
      "allow_access": {
        "Type": "Set",
        "Required": true,
        "Description": "An array of resources allowed to access the database. Accepted values include `any`, `IPv4 address`, `server identifier`, `server group identifier`",
        "MinItems": 1,
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "database_engine": {
        "Type": "String",
        "Optional": true,
        "Description": "The DBMS engine of the Database Server",
        "Computed": true
      },
      "database_type": {
        "Type": "String",
        "Optional": true,
        "Description": "ID of the database type to use",
        "Computed": true
      },
      "database_version": {
        "Type": "String",
        "Optional": true,
        "Description": "The version of the given engine in use",
        "Computed": true
      },
      "description": {
        "Type": "String",
        "Optional": true,
        "Description": "Editable user label"
      },
      "locked": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Initial password required to login, only available at creation or following a password reset request",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "maintenance_hour": {
        "Type": "Int",
        "Optional": true,
        "Description": "Number representing 24hr time start of maintenance window hour for x:00-x:59 (0-23)",
        "Computed": true
      },
      "maintenance_weekday": {
        "Type": "Int",
        "Optional": true,
        "Description": "Numerical index of weekday (0 is Sunday, 1 is Monday...) to set when automatic updates may be performed",
        "Computed": true
      },
      "name": {
        "Type": "String",
        "Optional": true,
        "Description": "Editable user label"
      },
      "snapshot": {
        "Type": "String",
        "Optional": true,
        "Description": "Identifier for an SQL snapshot to use as the basis of the new instance. Creates and restores the database from the snapshot"
      },
      "snapshots_schedule": {
        "Type": "String",
        "Optional": true,
        "Description": "Crontab pattern for scheduled snapshots. Must be at least hourly",
        "Default": {
          "Type": "string",
          "Value": "0 7 * * *"
        }
      },
      "snapshots_schedule_next_at": {
        "Type": "String",
        "Description": "time in UTC when next approximate scheduled snapshot will be run",
        "Computed": true
      },
      "status": {
        "Type": "String",
        "Description": "State the database server is in",
        "Computed": true
      },
      "zone": {
        "Type": "String",
        "Optional": true,
        "Description": "ID of the zone the database server is in",
        "Computed": true
      }
    },
    "brightbox_firewall_policy": {
      "__timeouts__": [
        "update"
      ],
      "description": {
        "Type": "String",
        "Optional": true,
        "Description": "Optional description of the policy"
      },
      "name": {
        "Type": "String",
        "Optional": true,
        "Description": "Optional name for this policy"
      },
      "server_group": {
        "Type": "String",
        "Optional": true,
        "Description": "The server group using this policy"
      }
    },
    "brightbox_firewall_rule": {
      "__timeouts__": [
        "create",
        "delete"
      ],
      "description": {
        "Type": "String",
        "Optional": true,
        "Description": "User editable label"
      },
      "destination": {
        "Type": "String",
        "Optional": true,
        "Description": "Subnet, ServerGroup or ServerID",
        "ConflictsWith": [
          "source"
        ]
      },
      "destination_port": {
        "Type": "String",
        "Optional": true,
        "Description": "single port, multiple ports or range separated by '-' or ':'; upto 255 characters example - '80', '80,443,21' or '3000-3999'"
      },
      "firewall_policy": {
        "Type": "String",
        "Required": true,
        "Description": "The firewall policy this rule is linked to"
      },
      "icmp_type_name": {
        "Type": "String",
        "Optional": true,
        "Description": "ICMP type name. 'echo-request', 'echo-reply'"
      },
      "protocol": {
        "Type": "String",
        "Optional": true,
        "Description": "Protocol Number, or one of tcp, udp, icmp"
      },
      "source": {
        "Type": "String",
        "Optional": true,
        "Description": "Subnet, ServerGroup or ServerID",
        "ConflictsWith": [
          "destination"
        ]
      },
      "source_port": {
        "Type": "String",
        "Optional": true,
        "Description": "single port, multiple ports or range separated by '-' or ':'; upto 255 characters example - '80', '80,443,21' or '3000-3999'"
      }
    },
    "brightbox_load_balancer": {
      "__timeouts__": [
        "create",
        "delete"
      ],
      "buffer_size": {
        "Type": "Int",
        "Optional": true,
        "Description": "Buffer size in bytes",
        "Computed": true
      },
      "certificate_pem": {
        "Type": "String",
        "Optional": true,
        "Description": "A X509 SSL certificate in PEM format"
      },
      "certificate_private_key": {
        "Type": "String",
        "Optional": true,
        "Description": "RSA private key used to sign the certificate in PEM format"
      },
      "domains": {
        "Type": "Set",
        "Optional": true,
        "Description": "Array of domain names to attempt to register with ACME",
        "ConflictsWith": [
          "certificate_pem",
          "certificate_private_key"
        ],
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "healthcheck": {
        "Type": "List",
        "Required": true,
        "Description": "Healthcheck options",
        "MaxItems": 1,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "interval": {
              "Type": "Int",
              "Optional": true,
              "Description": "How often to check in milliseconds",
              "Computed": true
            },
            "port": {
              "Type": "Int",
              "Required": true,
              "Description": "Port on server to connect to for healthcheck"
            },
            "request": {
              "Type": "String",
              "Optional": true,
              "Description": "HTTP path to check if http type healthcheck",
              "Computed": true
            },
            "threshold_down": {
              "Type": "Int",
              "Optional": true,
              "Description": "How many checks have to fail before the load balancers considers a server inactive",
              "Computed": true
            },
            "threshold_up": {
              "Type": "Int",
              "Optional": true,
              "Description": "How many checks have to pass before the load balancer considers the server active",
              "Computed": true
            },
            "timeout": {
              "Type": "Int",
              "Optional": true,
              "Description": "How long to wait for a response before marking the check as a fail",
              "Computed": true
            },
            "type": {
              "Type": "String",
              "Required": true,
              "Description": "Protocol type to check (tcp/http)"
            }
          }
        }
      },
      "https_redirect": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Redirect any requests on port 80 automatically to port 443",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "listener": {
        "Type": "Set",
        "Required": true,
        "Description": "Array of listeners",
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "in": {
              "Type": "Int",
              "Required": true,
              "Description": "The port this listener listens on"
            },
            "out": {
              "Type": "Int",
              "Required": true,
              "Description": "The port on this server the listener should talk to"
            },
            "protocol": {
              "Type": "String",
              "Required": true,
              "Description": "The protocol to load balance (http/tcp)"
            },
            "proxy_protocol": {
              "Type": "String",
              "Optional": true,
              "Description": "The version of the Proxy Protocol supported by the backend servers"
            },
            "timeout": {
              "Type": "Int",
              "Optional": true,
              "Description": "Connection timeout in milliseconds",
              "Default": {
                "Type": "int",
                "Value": "50000"
              }
            }
          }
        }
      },
      "locked": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Is true if resource has been set as locked and can not be deleted",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "name": {
        "Type": "String",
        "Optional": true,
        "Description": "Editable user label"
      },
      "nodes": {
        "Type": "Set",
        "Optional": true,
        "Description": "IDs of servers connected to this load balancer",
        "Computed": true,
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "policy": {
        "Type": "String",
        "Optional": true,
        "Description": "Method of load balancing. Supports `least-connections`, `round-robin` or `source-address`)",
        "Computed": true
      },
      "ssl_minimum_version": {
        "Type": "String",
        "Optional": true,
        "Description": "The minimum TLS/SSL version for the load balancer to accept. Supports `TLSv1.0`, `TLSv1.1`, `TLSv1.2`, `TLSv1.3` and `SSLv3`",
        "Computed": true
      },
      "sslv3": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Allow SSLv3 to be used",
        "Deprecated": "No longer supported. Will always return false",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "status": {
        "Type": "String",
        "Description": "Current state of the load balancer",
        "Computed": true
      }
    },
    "brightbox_orbit_container": {
      "__timeouts__": [
        "create",
        "delete"
      ],
      "bytes_used": {
        "Type": "Int",
        "Description": "Number of bytes used by the container",
        "Computed": true
      },
      "container_read": {
        "Type": "Set",
        "Optional": true,
        "Description": "Who can read the container",
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "container_sync_key": {
        "Type": "String",
        "Optional": true,
        "Description": "Container sync key"
      },
      "container_sync_to": {
        "Type": "String",
        "Optional": true,
        "Description": "Container to sync to"
      },
      "container_write": {
        "Type": "Set",
        "Optional": true,
        "Description": "Who can write to the container",
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "created_at": {
        "Type": "String",
        "Description": "The time the container was created",
        "Computed": true
      },
      "history_location": {
        "Type": "String",
        "Optional": true,
        "Description": "History location",
        "ConflictsWith": [
          "versions_location"
        ]
      },
      "metadata": {
        "Type": "Map",
        "Optional": true,
        "Description": "Set of key/value metadata associated with the container",
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "name": {
        "Type": "String",
        "Required": true,
        "Description": "Name of the Container"
      },
      "object_count": {
        "Type": "Int",
        "Description": "Number of objects in the container",
        "Computed": true
      },
      "storage_policy": {
        "Type": "String",
        "Description": "Any storage policy in place",
        "Computed": true
      },
      "versions_location": {
        "Type": "String",
        "Optional": true,
        "Description": "Versions Location",
        "ConflictsWith": [
          "history_location"
        ]
      }
    },
    "brightbox_server": {
      "__timeouts__": [
        "create",
        "delete"
      ],
      "disk_encrypted": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Is true if the server has been built with an encrypted disk",
        "Computed": true
      },
      "fqdn": {
        "Type": "String",
        "Description": "Fully qualified domain name",
        "Computed": true
      },
      "hostname": {
        "Type": "String",
        "Description": "Short hostname",
        "Computed": true
      },
      "image": {
        "Type": "String",
        "Required": true,
        "Description": "Image used to create the server"
      },
      "interface": {
        "Type": "String",
        "Description": "Network Interface connected to this server",
        "Computed": true
      },
      "ipv4_address": {
        "Type": "String",
        "Description": "Public IPv4 address of the interface",
        "Computed": true
      },
      "ipv4_address_private": {
        "Type": "String",
        "Description": "Private IPv4 address of the interface",
        "Computed": true
      },
      "ipv6_address": {
        "Type": "String",
        "Description": "Public IPv6 address of the interface",
        "Computed": true
      },
      "ipv6_hostname": {
        "Type": "String",
        "Description": "Public IPv6 FQDN",
        "Computed": true
      },
      "locked": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Is true if resource has been set as locked and cannot be deleted",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "name": {
        "Type": "String",
        "Optional": true,
        "Description": "Editable user label"
      },
      "public_hostname": {
        "Type": "String",
        "Description": "Public IPv4 FQDN",
        "Computed": true
      },
      "server_groups": {
        "Type": "Set",
        "Required": true,
        "Description": "Array of server groups to add server to",
        "MinItems": 1,
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "status": {
        "Type": "String",
        "Description": "Current state of server",
        "Computed": true
      },
      "type": {
        "Type": "String",
        "Optional": true,
        "Description": "Server type of the server",
        "Computed": true
      },
      "user_data": {
        "Type": "String",
        "Optional": true,
        "Description": "Data made available to Cloud Init",
        "ConflictsWith": [
          "user_data_base64"
        ]
      },
      "user_data_base64": {
        "Type": "String",
        "Optional": true,
        "Description": "Base64 encoded data made available to Cloud Init",
        "ConflictsWith": [
          "user_data"
        ]
      },
      "username": {
        "Type": "String",
        "Description": "Username to use when logging into a server",
        "Computed": true
      },
      "zone": {
        "Type": "String",
        "Optional": true,
        "Description": "Zone where server is located",
        "Computed": true
      }
    },
    "brightbox_server_group": {
      "__timeouts__": [
        "create",
        "delete"
      ],
      "default": {
        "Type": "Bool",
        "Description": "Is this the default group for the account?",
        "Computed": true
      },
      "description": {
        "Type": "String",
        "Optional": true,
        "Description": "User editable label"
      },
      "fqdn": {
        "Type": "String",
        "Description": "Fully Qualified Domain Name",
        "Computed": true
      },
      "name": {
        "Type": "String",
        "Optional": true,
        "Description": "User editable label"
      }
    }
  },
  "data-sources": {
    "brightbox_database_type": {
      "description": {
        "Type": "String",
        "Optional": true,
        "Description": "Description of this database type",
        "Computed": true
      },
      "disk_size": {
        "Type": "Int",
        "Description": "Disk size in megabytes",
        "Computed": true
      },
      "name": {
        "Type": "String",
        "Optional": true,
        "Description": "Name of this database type",
        "Computed": true
      },
      "ram": {
        "Type": "Int",
        "Description": "RAM size in megabytes",
        "Computed": true
      }
    },
    "brightbox_image": {
      "ancestor_id": {
        "Type": "String",
        "Optional": true,
        "Description": "Image this image was derived from",
        "Computed": true
      },
      "arch": {
        "Type": "String",
        "Optional": true,
        "Description": "OS Architecture",
        "Computed": true
      },
      "compatibility_mode": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Does this image require a non-virtio VM shell",
        "Computed": true
      },
      "created_at": {
        "Type": "String",
        "Description": "The time this image was created/registered (UTC)",
        "Computed": true
      },
      "description": {
        "Type": "String",
        "Optional": true,
        "Description": "A Description of the image",
        "Computed": true
      },
      "disk_size": {
        "Type": "Int",
        "Description": "The actual size of the data within this image in Megabytes",
        "Computed": true
      },
      "licence_name": {
        "Type": "String",
        "Optional": true,
        "Description": "The licence name for this image",
        "Computed": true
      },
      "locked": {
        "Type": "Bool",
        "Description": "Is true if the image is set as locked and cannot be deleted",
        "Computed": true
      },
      "most_recent": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Select the most recent image",
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "name": {
        "Type": "String",
        "Optional": true,
        "Description": "User Label for this image",
        "Computed": true
      },
      "official": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Is this image an official Brightbox provided one?",
        "Computed": true
      },
      "owner": {
        "Type": "String",
        "Optional": true,
        "Description": "Account ID this image belongs to",
        "Computed": true
      },
      "public": {
        "Type": "Bool",
        "Optional": true,
        "Description": "Is this image available to other customers?",
        "Computed": true
      },
      "source_type": {
        "Type": "String",
        "Optional": true,
        "Description": "Source type for this image (upload or snapshot)",
        "Computed": true
      },
      "status": {
        "Type": "String",
        "Optional": true,
        "Description": "State of the image",
        "Computed": true
      },
      "username": {
        "Type": "String",
        "Optional": true,
        "Description": "Username to use when logging into a server booted with this image",
        "Computed": true
      },
      "virtual_size": {
        "Type": "Int",
