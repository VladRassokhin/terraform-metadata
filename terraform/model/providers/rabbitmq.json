{
  ".schema_version": "2",
  ".sdk_type": "terraform-plugin-sdk-v1",
  "name": "rabbitmq",
  "type": "provider",
  "version": "v1.4.0",
  "provider": {
    "cacert_file": {
      "Type": "String",
      "Optional": true,
      "DefaultFunc": "ENV(RABBITMQ_CACERT)"
    },
    "clientcert_file": {
      "Type": "String",
      "Optional": true,
      "DefaultFunc": "ENV(RABBITMQ_CLIENTCERT)"
    },
    "clientkey_file": {
      "Type": "String",
      "Optional": true,
      "DefaultFunc": "ENV(RABBITMQ_CLIENTKEY)"
    },
    "endpoint": {
      "Type": "String",
      "Required": true,
      "DefaultFunc": "ENV(RABBITMQ_ENDPOINT)"
    },
    "insecure": {
      "Type": "Bool",
      "Optional": true,
      "DefaultFunc": "ENV(RABBITMQ_INSECURE)"
    },
    "password": {
      "Type": "String",
      "Required": true,
      "DefaultFunc": "ENV(RABBITMQ_PASSWORD)"
    },
    "username": {
      "Type": "String",
      "Required": true,
      "DefaultFunc": "ENV(RABBITMQ_USERNAME)"
    }
  },
  "resources": {
    "rabbitmq_binding": {
      "arguments": {
        "Type": "Map",
        "Optional": true,
        "ConflictsWith": [
          "arguments_json"
        ]
      },
      "arguments_json": {
        "Type": "String",
        "Optional": true,
        "ConflictsWith": [
          "arguments"
        ]
      },
      "destination": {
        "Type": "String",
        "Required": true
      },
      "destination_type": {
        "Type": "String",
        "Required": true
      },
      "properties_key": {
        "Type": "String",
        "Computed": true
      },
      "routing_key": {
        "Type": "String",
        "Optional": true
      },
      "source": {
        "Type": "String",
        "Required": true
      },
      "vhost": {
        "Type": "String",
        "Required": true
      }
    },
    "rabbitmq_exchange": {
      "name": {
        "Type": "String",
        "Required": true
      },
      "settings": {
        "Type": "List",
        "Required": true,
        "MaxItems": 1,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "arguments": {
              "Type": "Map",
              "Optional": true
            },
            "auto_delete": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "durable": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "type": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "vhost": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "/"
        }
      }
    },
    "rabbitmq_federation_upstream": {
      "component": {
        "Type": "String",
        "Computed": true
      },
      "definition": {
        "Type": "List",
        "Required": true,
        "MaxItems": 1,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "ack_mode": {
              "Type": "String",
              "Optional": true,
              "Default": {
                "Type": "string",
                "Value": "on-confirm"
              }
            },
            "exchange": {
              "Type": "String",
              "Optional": true
            },
            "expires": {
              "Type": "Int",
              "Optional": true
            },
            "max_hops": {
              "Type": "Int",
              "Optional": true,
              "Default": {
                "Type": "int",
                "Value": "1"
              }
            },
            "message_ttl": {
              "Type": "Int",
              "Optional": true
            },
            "prefetch_count": {
              "Type": "Int",
              "Optional": true,
              "Default": {
                "Type": "int",
                "Value": "1000"
              }
            },
            "queue": {
              "Type": "String",
              "Optional": true
            },
            "reconnect_delay": {
              "Type": "Int",
              "Optional": true,
              "Default": {
                "Type": "int",
                "Value": "5"
              }
            },
            "trust_user_id": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "uri": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "vhost": {
        "Type": "String",
        "Required": true
      }
    },
    "rabbitmq_permissions": {
      "permissions": {
        "Type": "List",
        "Required": true,
        "MaxItems": 1,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "configure": {
              "Type": "String",
              "Required": true
            },
            "read": {
              "Type": "String",
              "Required": true
            },
            "write": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "user": {
        "Type": "String",
        "Required": true
      },
      "vhost": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "/"
        }
      }
    },
    "rabbitmq_policy": {
      "name": {
        "Type": "String",
        "Required": true
      },
      "policy": {
        "Type": "List",
        "Required": true,
        "MaxItems": 1,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "apply_to": {
              "Type": "String",
              "Required": true
            },
            "definition": {
              "Type": "Map",
              "Required": true
            },
            "pattern": {
              "Type": "String",
              "Required": true
            },
            "priority": {
              "Type": "Int",
              "Required": true
            }
          }
        }
      },
      "vhost": {
        "Type": "String",
        "Required": true
      }
    },
    "rabbitmq_queue": {
      "name": {
        "Type": "String",
        "Required": true
      },
      "settings": {
        "Type": "List",
        "Required": true,
        "MaxItems": 1,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "arguments": {
              "Type": "Map",
              "Optional": true,
              "ConflictsWith": [
                "settings.0.arguments_json"
              ]
            },
            "arguments_json": {
              "Type": "String",
              "Optional": true,
              "ConflictsWith": [
                "settings.0.arguments"
              ]
            },
            "auto_delete": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "durable": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            }
          }
        }
      },
      "vhost": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "/"
        }
      }
    },
    "rabbitmq_shovel": {
      "info": {
        "Type": "List",
        "Required": true,
        "MaxItems": 1,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "ack_mode": {
              "Type": "String",
              "Optional": true,
              "Default": {
                "Type": "string",
                "Value": "on-confirm"
              }
            },
            "add_forward_headers": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "delete_after": {
              "Type": "String",
              "Optional": true,
              "Default": {
                "Type": "string",
                "Value": "never"
              }
            },
            "destination_exchange": {
              "Type": "String",
              "Optional": true
            },
            "destination_exchange_key": {
              "Type": "String",
              "Optional": true
            },
            "destination_queue": {
              "Type": "String",
              "Optional": true
            },
            "destination_uri": {
              "Type": "String",
              "Required": true
            },
            "prefetch_count": {
              "Type": "Int",
              "Optional": true,
              "Default": {
                "Type": "int",
                "Value": "1000"
              }
            },
            "reconnect_delay": {
              "Type": "Int",
              "Optional": true,
              "Default": {
                "Type": "int",
                "Value": "1"
              }
            },
            "source_exchange": {
              "Type": "String",
              "Optional": true
            },
            "source_exchange_key": {
              "Type": "String",
              "Optional": true
            },
            "source_queue": {
              "Type": "String",
              "Optional": true
            },
            "source_uri": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "vhost": {
        "Type": "String",
        "Required": true
      }
    },
    "rabbitmq_topic_permissions": {
      "permissions": {
        "Type": "Set",
        "Required": true,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "exchange": {
              "Type": "String",
              "Required": true
            },
            "read": {
              "Type": "String",
              "Required": true
            },
            "write": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "user": {
        "Type": "String",
        "Required": true
      },
      "vhost": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "/"
        }
      }
    },
    "rabbitmq_user": {
      "name": {
        "Type": "String",
        "Required": true
      },
      "password": {
        "Type": "String",
        "Required": true
      },
      "tags": {
        "Type": "List",
        "Optional": true,
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      }
    },
    "rabbitmq_vhost": {
      "name": {
        "Type": "String",
        "Required": true
      }
    }
  },
  "data-sources": {}
}