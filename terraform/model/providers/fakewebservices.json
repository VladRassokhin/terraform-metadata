{
  ".schema_version": "2",
  ".sdk_type": "terraform-plugin-sdk-v2",
  "name": "fakewebservices",
  "type": "provider",
  "version": "v0.2.1",
  "provider": {
    "hostname": {
      "Type": "String",
      "Optional": true,
      "DefaultFunc": "ENV(FWS_HOSTNAME)"
    },
    "token": {
      "Type": "String",
      "Optional": true,
      "DefaultFunc": "UNKNOWN"
    }
  },
  "resources": {
    "fakewebservices_database": {
      "name": {
        "Type": "String",
        "Required": true,
        "Description": "The name of the database."
      },
      "size": {
        "Type": "Int",
        "Required": true,
        "Description": "The allocated size of the database in gigabytes."
      }
    },
    "fakewebservices_load_balancer": {
      "name": {
        "Type": "String",
        "Required": true,
        "Description": "The name of the load balancer."
      },
      "servers": {
        "Type": "Set",
        "Optional": true,
        "Description": "A list of server names to attach to the load balancer.",
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      }
    },
    "fakewebservices_server": {
      "name": {
        "Type": "String",
        "Required": true,
        "Description": "The name of the server."
      },
      "type": {
        "Type": "String",
        "Required": true,
        "Description": "The server type."
      },
      "vpc": {
        "Type": "String",
        "Optional": true,
        "Description": "The name of the VPC to deploy this server in."
      }
    },
    "fakewebservices_vpc": {
      "cidr_block": {
        "Type": "String",
        "Required": true,
        "Description": "The range of IPv4 addresses for this VPC, in the form of a Classless Inter-Domain Routing (CIDR) block."
      },
      "name": {
        "Type": "String",
        "Required": true,
        "Description": "The name of the VPC."
      }
    }
  },
  "data-sources": {}
}