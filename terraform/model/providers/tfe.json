        }
      }
    },
    "tfe_registry_module": {
      "module_provider": {
        "Type": "String",
        "Computed": true
      },
      "name": {
        "Type": "String",
        "Computed": true
      },
      "organization": {
        "Type": "String",
        "Computed": true
      },
      "vcs_repo": {
        "Type": "List",
        "Required": true,
        "MaxItems": 1,
        "MinItems": 1,
        "IsBlock": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "display_identifier": {
              "Type": "String",
              "Required": true
            },
            "identifier": {
              "Type": "String",
              "Required": true
            },
            "oauth_token_id": {
              "Type": "String",
              "Required": true
            }
          }
        }
      }
    },
    "tfe_run_trigger": {
      "sourceable_id": {
        "Type": "String",
        "Required": true
      },
      "workspace_external_id": {
        "Type": "String",
        "Optional": true,
        "Computed": true,
        "ConflictsWith": [
          "workspace_id"
        ],
        "Deprecated": "Use workspace_id instead. The workspace_external_id attribute will be removed in the future. See the CHANGELOG to learn more: https://github.com/terraform-providers/terraform-provider-tfe/blob/v0.18.0/CHANGELOG.md"
      },
      "workspace_id": {
        "Type": "String",
        "Optional": true,
        "Computed": true,
        "ConflictsWith": [
          "workspace_external_id"
        ]
      }
    },
    "tfe_sentinel_policy": {
      "description": {
        "Type": "String",
        "Optional": true,
        "Computed": true
      },
      "enforce_mode": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "soft-mandatory"
        }
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "organization": {
        "Type": "String",
        "Required": true
      },
      "policy": {
        "Type": "String",
        "Required": true
      }
    },
    "tfe_ssh_key": {
      "key": {
        "Type": "String",
        "Required": true
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "organization": {
        "Type": "String",
        "Required": true
      }
    },
    "tfe_team": {
      "name": {
        "Type": "String",
        "Required": true
      },
      "organization": {
        "Type": "String",
        "Required": true
      },
      "organization_access": {
        "Type": "List",
        "Optional": true,
        "Computed": true,
        "MaxItems": 1,
        "IsBlock": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "manage_policies": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "manage_vcs_settings": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "manage_workspaces": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            }
          }
        }
      },
      "visibility": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "secret"
        }
      }
    },
    "tfe_team_access": {
      "access": {
        "Type": "String",
        "Optional": true,
        "Computed": true
      },
      "permissions": {
        "Type": "List",
        "Optional": true,
        "Computed": true,
        "MaxItems": 1,
        "IsBlock": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "runs": {
              "Type": "String",
              "Required": true
            },
            "sentinel_mocks": {
              "Type": "String",
