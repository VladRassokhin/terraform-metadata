    "tfe_team_members": {
      "team_id": {
        "Type": "String",
        "Required": true
      },
      "usernames": {
        "Type": "Set",
        "Required": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      }
    },
    "tfe_team_organization_member": {
      "organization_membership_id": {
        "Type": "String",
        "Required": true
      },
      "team_id": {
        "Type": "String",
        "Required": true
      }
    },
    "tfe_team_token": {
      "force_regenerate": {
        "Type": "Bool",
        "Optional": true
      },
      "team_id": {
        "Type": "String",
        "Required": true
      },
      "token": {
        "Type": "String",
        "Computed": true
      }
    },
    "tfe_variable": {
      "category": {
        "Type": "String",
        "Required": true
      },
      "description": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string"
        }
      },
      "hcl": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "key": {
        "Type": "String",
        "Required": true
      },
      "sensitive": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "value": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string"
        }
      },
      "workspace_id": {
        "Type": "String",
        "Required": true
      }
    },
    "tfe_workspace": {
      "auto_apply": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "external_id": {
        "Type": "String",
        "Computed": true
      },
      "file_triggers_enabled": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "true"
        }
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "operations": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "true"
        }
      },
      "organization": {
        "Type": "String",
        "Required": true
      },
      "queue_all_runs": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "true"
        }
      },
      "ssh_key_id": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string"
        }
      },
      "terraform_version": {
        "Type": "String",
        "Optional": true,
        "Computed": true
      },
      "trigger_prefixes": {
        "Type": "List",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "vcs_repo": {
        "Type": "List",
        "Optional": true,
        "MaxItems": 1,
        "MinItems": 1,
        "IsBlock": true,
