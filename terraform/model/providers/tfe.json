{
  ".schema_version": "1",
  "name": "tfe",
  "type": "provider",
  "version": "v0.20.0",
  "provider": {
    "hostname": {
      "Type": "String",
      "Optional": true,
      "Description": "The Terraform Enterprise hostname to connect to. Defaults to app.terraform.io.",
      "DefaultFunc": "ENV(TFE_HOSTNAME)"
    },
    "token": {
      "Type": "String",
      "Optional": true,
      "Description": "The token used to authenticate with Terraform Enterprise. We recommend omitting\nthe token which can be set as credentials in the CLI config file.",
      "DefaultFunc": "ENV(TFE_TOKEN)"
    }
  },
  "resources": {
    "tfe_notification_configuration": {
      "destination_type": {
        "Type": "String",
        "Required": true
      },
      "email_addresses": {
        "Type": "Set",
        "Optional": true,
        "Computed": true,
        "ConflictsWith": [
          "token",
          "url"
        ],
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "email_user_ids": {
        "Type": "Set",
        "Optional": true,
        "Computed": true,
        "ConflictsWith": [
          "token",
          "url"
        ],
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "enabled": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "token": {
        "Type": "String",
        "Optional": true
      },
      "triggers": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "url": {
        "Type": "String",
        "Optional": true,
        "ConflictsWith": [
          "email_addresses",
          "email_user_ids"
        ]
      },
      "workspace_external_id": {
        "Type": "String",
        "Optional": true,
        "Computed": true,
        "ConflictsWith": [
          "workspace_id"
        ],
        "Deprecated": "Use workspace_id instead. The workspace_external_id attribute will be removed in the future. See the CHANGELOG to learn more: https://github.com/terraform-providers/terraform-provider-tfe/blob/v0.18.0/CHANGELOG.md"
      },
      "workspace_id": {
        "Type": "String",
        "Optional": true,
        "Computed": true,
        "ConflictsWith": [
          "workspace_external_id"
        ]
      }
    },
    "tfe_oauth_client": {
      "api_url": {
        "Type": "String",
        "Required": true
      },
      "http_url": {
        "Type": "String",
        "Required": true
      },
      "oauth_token": {
        "Type": "String",
        "Required": true
      },
      "oauth_token_id": {
        "Type": "String",
        "Computed": true
      },
      "organization": {
        "Type": "String",
        "Required": true
      },
      "private_key": {
        "Type": "String",
        "Optional": true
      },
      "service_provider": {
        "Type": "String",
        "Required": true
      }
    },
    "tfe_organization": {
      "collaborator_auth_policy": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "password"
        }
      },
      "email": {
        "Type": "String",
        "Required": true
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "owners_team_saml_role_id": {
        "Type": "String",
        "Optional": true
      },
      "session_remember_minutes": {
        "Type": "Int",
        "Optional": true
      },
      "session_timeout_minutes": {
        "Type": "Int",
        "Optional": true
      }
    },
    "tfe_organization_membership": {
      "email": {
        "Type": "String",
        "Required": true
      },
      "organization": {
        "Type": "String",
        "Required": true
      },
      "user_id": {
        "Type": "String",
        "Computed": true
      }
    },
    "tfe_organization_token": {
      "force_regenerate": {
        "Type": "Bool",
        "Optional": true
      },
      "organization": {
        "Type": "String",
        "Required": true
      },
      "token": {
        "Type": "String",
        "Computed": true
      }
    },
    "tfe_policy_set": {
      "description": {
        "Type": "String",
        "Optional": true,
        "Computed": true
      },
      "global": {
        "Type": "Bool",
        "Optional": true,
        "ConflictsWith": [
          "workspace_external_ids"
        ],
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "organization": {
        "Type": "String",
        "Required": true
      },
      "policies_path": {
        "Type": "String",
        "Optional": true,
        "ConflictsWith": [
          "policy_ids"
        ]
      },
      "policy_ids": {
        "Type": "Set",
        "Optional": true,
        "ConflictsWith": [
          "vcs_repo",
          "policies_path"
        ],
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "vcs_repo": {
        "Type": "List",
        "Optional": true,
        "MaxItems": 1,
        "MinItems": 1,
        "ConflictsWith": [
          "policy_ids"
        ],
        "IsBlock": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "branch": {
              "Type": "String",
              "Optional": true
            },
            "identifier": {
              "Type": "String",
              "Required": true
            },
            "ingress_submodules": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "oauth_token_id": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "workspace_external_ids": {
        "Type": "Set",
        "Optional": true,
        "Computed": true,
        "ConflictsWith": [
          "global",
          "workspace_ids"
        ],
        "Deprecated": "Use workspace_ids instead. The workspace_external_ids attribute will be removed in the future. See the CHANGELOG to learn more: https://github.com/terraform-providers/terraform-provider-tfe/blob/v0.18.0/CHANGELOG.md",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "workspace_ids": {
        "Type": "Set",
        "Optional": true,
        "Computed": true,
        "ConflictsWith": [
          "global",
          "workspace_external_ids"
        ],
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      }
    },
