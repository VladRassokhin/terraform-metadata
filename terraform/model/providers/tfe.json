            },
            "identifier": {
              "Type": "String",
              "Required": true
            },
            "ingress_submodules": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "oauth_token_id": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "working_directory": {
        "Type": "String",
