              "Deprecated": "Use enforce_admins instead",
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "require_code_owner_reviews": {
              "Type": "Bool",
              "Optional": true
            },
            "required_approving_review_count": {
              "Type": "Int",
              "Optional": true,
              "Default": {
                "Type": "int",
                "Value": "1"
              }
            }
          }
        }
      },
      "required_status_checks": {
        "Type": "List",
        "Optional": true,
        "MaxItems": 1,
        "IsBlock": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "contexts": {
              "Type": "Set",
