    "auth_token": {
      "Type": "String",
      "Required": true,
      "Description": "The API auth key for API operations.",
      "DefaultFunc": "MENV(METAL_AUTH_TOKEN,PACKET_AUTH_TOKEN)"
    },
    "max_retries": {
      "Type": "Int",
      "Optional": true,
      "Default": {
        "Type": "int",
        "Value": "10"
      }
    },
    "max_retry_wait_seconds": {
      "Type": "Int",
      "Optional": true,
      "Default": {
        "Type": "int",
        "Value": "30"
      }
    }
  },
  "resources": {
    "metal_bgp_session": {
      "address_family": {
        "Type": "String",
        "Required": true
      },
      "default_route": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "device_id": {
        "Type": "String",
        "Required": true
      },
      "status": {
        "Type": "String",
        "Computed": true
      }
    },
    "metal_device": {
      "__timeouts__": [
        "create",
        "update",
        "delete"
      ],
      "access_private_ipv4": {
        "Type": "String",
        "Computed": true
      },
      "access_public_ipv4": {
        "Type": "String",
        "Computed": true
      },
      "access_public_ipv6": {
        "Type": "String",
        "Computed": true
      },
      "always_pxe": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "billing_cycle": {
        "Type": "String",
        "Required": true
      },
      "created": {
        "Type": "String",
        "Computed": true
      },
      "custom_data": {
        "Type": "String",
        "Optional": true
      },
      "deployed_facility": {
        "Type": "String",
        "Computed": true
      },
      "deployed_hardware_reservation_id": {
        "Type": "String",
        "Computed": true
      },
      "description": {
        "Type": "String",
        "Optional": true
      },
      "facilities": {
        "Type": "List",
        "Optional": true,
        "MinItems": 1,
        "ConflictsWith": [
          "metro"
        ],
        "Deprecated": "Use metro attribute instead",
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "force_detach_volumes": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "hardware_reservation_id": {
        "Type": "String",
        "Optional": true
      },
      "hostname": {
        "Type": "String",
        "Required": true
      },
      "ip_address": {
        "Type": "List",
        "Optional": true,
        "Description": "Inbound rules for this security group",
        "MinItems": 1,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "cidr": {
              "Type": "Int",
              "Optional": true,
              "Description": "CIDR suffix for IP block assigned to this device"
            },
            "reservation_ids": {
              "Type": "List",
              "Optional": true,
              "Description": "IDs of reservations to pick the blocks from",
              "MinItems": 1,
              "ConfigImplicitMode": "Attr",
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            },
            "type": {
              "Type": "String",
              "Required": true,
              "Description": "one of public_ipv4,private_ipv4,public_ipv6"
            }
          }
        }
      },
      "ipxe_script_url": {
        "Type": "String",
        "Optional": true
      },
      "locked": {
        "Type": "Bool",
        "Computed": true
      },
      "metro": {
        "Type": "String",
        "Optional": true,
        "ConflictsWith": [
          "facilities"
        ]
      },
      "network": {
        "Type": "List",
        "Computed": true,
        "IsBlock": true,
        "ConfigImplicitMode": "ComputedBlock",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "address": {
              "Type": "String",
              "Computed": true
            },
            "cidr": {
              "Type": "Int",
              "Computed": true
            },
            "family": {
              "Type": "Int",
              "Computed": true
            },
            "gateway": {
              "Type": "String",
              "Computed": true
            },
            "public": {
              "Type": "Bool",
              "Computed": true
            }
          }
        }
      },
      "network_type": {
        "Type": "String",
        "Computed": true,
        "Deprecated": "You should handle Network Type with the new metal_device_network_type resource."
      },
      "operating_system": {
        "Type": "String",
        "Required": true
      },
      "plan": {
        "Type": "String",
        "Required": true
      },
      "ports": {
        "Type": "List",
        "Computed": true,
        "IsBlock": true,
        "ConfigImplicitMode": "ComputedBlock",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "bonded": {
              "Type": "Bool",
              "Computed": true
            },
            "id": {
              "Type": "String",
              "Computed": true
            },
            "mac": {
              "Type": "String",
              "Computed": true
            },
            "name": {
              "Type": "String",
              "Computed": true
            },
            "type": {
              "Type": "String",
              "Computed": true
            }
          }
        }
      },
      "project_id": {
        "Type": "String",
        "Required": true
      },
      "project_ssh_key_ids": {
        "Type": "List",
        "Optional": true,
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "root_password": {
        "Type": "String",
        "Computed": true
      },
      "ssh_key_ids": {
        "Type": "List",
        "Computed": true,
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "state": {
        "Type": "String",
        "Computed": true
      },
      "storage": {
        "Type": "String",
        "Optional": true
      },
      "tags": {
        "Type": "List",
        "Optional": true,
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "updated": {
        "Type": "String",
        "Computed": true
      },
      "user_data": {
        "Type": "String",
        "Optional": true
      },
