      "manageable": {
        "Type": "Bool",
        "Computed": true
      },
      "management": {
        "Type": "Bool",
        "Computed": true
      },
      "netmask": {
        "Type": "String",
        "Description": "Mask in decimal notation, e.g. 255.255.255.0",
        "Computed": true
      },
      "network": {
        "Type": "String",
        "Description": "Network IP address portion of the block specification",
        "Computed": true
      },
      "public": {
        "Type": "Bool",
        "Description": "Flag indicating whether IP block is addressable from the Internet",
        "Computed": true
      }
    },
    "metal_organization": {
      "created": {
        "Type": "String",
        "Computed": true
      },
      "description": {
        "Type": "String",
