        "Type": "String",
        "Optional": true,
        "Computed": true
      },
      "tags": {
        "Type": "Set",
        "Optional": true,
        "Computed": true,
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
