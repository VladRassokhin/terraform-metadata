        "Type": "List",
        "Computed": true,
        "IsBlock": true,
        "ConfigImplicitMode": "ComputedBlock",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "resource_uri": {
              "Type": "String",
              "Computed": true
            },
            "uuid": {
