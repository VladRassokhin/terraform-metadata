            "ConflictsWith": [
              "update.0.key_name",
              "update.0.key_algorithm",
              "update.0.key_secret"
            ],
            "IsBlock": true,
            "ConfigImplicitMode": "Block",
            "Elem": {
              "Type": "SchemaInfo",
              "Info": {
                "keytab": {
                  "Type": "String",
                  "Optional": true,
                  "DefaultFunc": "ENV(DNS_UPDATE_KEYTAB)",
                  "ConflictsWith": [
                    "update.0.gssapi.0.password"
                  ]
                },
                "password": {
                  "Type": "String",
                  "Optional": true,
                  "DefaultFunc": "ENV(DNS_UPDATE_PASSWORD)",
                  "ConflictsWith": [
                    "update.0.gssapi.0.keytab"
                  ]
                },
                "realm": {
                  "Type": "String",
