                "username": {
                  "Type": "String",
                  "Optional": true,
                  "DefaultFunc": "ENV(DNS_UPDATE_USERNAME)"
                }
              }
            }
          },
          "key_algorithm": {
            "Type": "String",
            "Optional": true,
            "DefaultFunc": "ENV(DNS_UPDATE_KEYALGORITHM)",
            "ConflictsWith": [
              "update.0.gssapi.0"
            ]
          },
          "key_name": {
            "Type": "String",
            "Optional": true,
            "DefaultFunc": "ENV(DNS_UPDATE_KEYNAME)",
            "ConflictsWith": [
              "update.0.gssapi.0"
            ]
          },
          "key_secret": {
            "Type": "String",
            "Optional": true,
            "DefaultFunc": "ENV(DNS_UPDATE_KEYSECRET)",
            "ConflictsWith": [
              "update.0.gssapi.0"
            ]
          },
          "port": {
            "Type": "Int",
            "Optional": true,
            "DefaultFunc": "UNKNOWN"
          },
          "retries": {
            "Type": "Int",
            "Optional": true,
            "DefaultFunc": "UNKNOWN"
          },
          "server": {
            "Type": "String",
            "Required": true,
            "DefaultFunc": "ENV(DNS_UPDATE_SERVER)"
          },
          "timeout": {
            "Type": "String",
            "Optional": true,
            "DefaultFunc": "ENV(DNS_UPDATE_TIMEOUT)"
          },
          "transport": {
